AA,GAAA,IAAA,CAAA,GAAA,GAAA,IAAA,CAAA,IAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,GAAA;AACA;AAPA;AAAA;AAAA;AAAA;AAAA;;AAQA,cAAA,MAAA,GAAA;AACA,YAAA,KAAA,EAAA,CAAA,CAAA,KAAA,CAAA,KAAA,OAAA,EAAA,OAAA,CADA;AAEA;AACA,YAAA,GAAA,EAAA,GAHA;AAIA,YAAA,IAAA,EAAA,KAAA,mBAAA,CAAA,MAJA;AAKA,YAAA,SAAA,EAAA,IALA;AAMA,YAAA,QAAA,EAAA;AANA,WAAA;AAQA,UAAA,KAAA,CAAA,qBAAA,CAAA,MAAA;AACA,SAjBA,MAiBA;AACA,eAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA;AACA,OArBA,CAqBA,OAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA,EADA,CAEA;AACA;AACA;AAhCA,GA9BA;AAkEA,EAAA,KAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA;AACA,UAAA,GAAA,KAAA,EAAA,EAAA;AACA,aAAA,YAAA,GAAA,GAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,YAAA,GAAA,CAAA,GAAA,EAAA,GAAA,EAAA,GAAA,EAAA,GAAA,CAAA;AACA;AACA;AAPA;AAlEA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div class=\"card\">\n    <div class=\"margin-bottom-10 clearfix\" style=\"color: #666;line-height:32px\">共 {{buyerStatisticsList.total || 0}} 条数据，\n      <p v-show=\"showTotality.indexOf('2')>-1\" style=\"display:inline-block\">\n        <span class=\"blue-color\">充值</span>合计<span class=\"font-blod font-size16\"> ￥{{buyerStatisticsList.deposit_amount || 0.00}}</span>；\n      </p>\n      <p v-show=\"showTotality.indexOf('1')>-1\" style=\"display:inline-block\">\n        <span class=\"blue-color\">消费</span>合计<span class=\"font-blod font-size16\"> ￥{{buyerStatisticsList.pay_amount || 0.00}} </span>；\n      </p>\n      <p v-show=\"showTotality.indexOf('3')>-1\" style=\"display:inline-block\">\n        <span class=\"blue-color\">提现</span>合计<span class=\"font-blod font-size16\"> ￥{{buyerStatisticsList.withdraw_amount || 0.00}} </span>；\n      </p>\n      <p v-show=\"showTotality.indexOf('4')>-1\" style=\"display:inline-block\">\n        <span class=\"blue-color\">退款</span>合计<span class=\"font-blod font-size16\"> ￥{{buyerStatisticsList.refund_amount || 0.00}} </span>\n      </p>\n<!--      <Button class=\"pull-right\" type='primary' ghost icon=\"md-download\" @click=\"exportExcel\">导出EXCEL</Button>-->\n    </div>\n    <Table border stripe :columns='columns' :data='buyerStatisticsList.result' :loading=\"loading\">\n      <template slot=\"amount\" slot-scope=\"{ row }\">\n        <span class=\"orange-color\"> ￥{{row.amount}}</span>\n      </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport _ from 'underscore';\nimport excel from '@/libs/excel';\nimport * as dataConfig from '../dataConfig';\nexport default {\n  name: 'SearchTable',\n  props: {\n    startDate: String,\n    endDate: String,\n    tradeTypes: String\n  },\n  data() {\n    return {\n      columns: [],\n      showTotality: ['1','2','3','4'],\n    };\n  },\n\n  components: {},\n\n  computed: {\n    buyerStatisticsList() {\n      return this.$store.state.statistics.buyerStatisticsList;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n\n    },\n    // 导出\n    exportExcel: function() {\n      try {\n        if (this.columns.length>0) {\n          let filename = '交易统计表' + '_' + this.startDate + '-' + this.endDate;\n          let key = [];\n          for(let item of this.columns){\n            let tmp = ''\n            tmp = item.key? item.key: item.slot;\n            key.push(tmp);\n          }\n          const params = {\n            title: _.pluck(this.columns, 'title'),\n            // key: _.pluck(this.columns, 'key'),\n            key: key,\n            data: this.buyerStatisticsList.result,\n            autoWidth: true,\n            filename: filename,\n          };\n          excel.export_array_to_excel(params);\n        } else {\n          this.$Message.info('表格数据不能为空！');\n        }\n      } catch (e) {\n        console.log(e);\n        // this.$store.commit('STAT_DAY_SET_PROPERTY_BY_KEY', { key: 'exportLoading', value: false })\n      }\n    },\n\n  },\n\n  watch:{\n    tradeTypes(val){\n      if(val!==''){\n        this.showTotality = val.split(',');\n      }else{\n        this.showTotality = ['1','2','3','4'];\n      }\n    }\n  }\n}\n</script>\n<style lang='less' scoped>\n    /deep/.ivu-table td.order_amount{\n        color: #ff602e;\n    }\n</style>\n"],"sourceRoot":"src/views/statistics/reserve-stat/components"}]}