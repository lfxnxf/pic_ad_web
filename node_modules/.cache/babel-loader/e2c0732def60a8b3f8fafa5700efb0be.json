Appointment(state, val) {\n      state.statAppointment = val;\n    },\n    UpdateStatOrder(state, val) {\n      state.statOrder = val;\n    },\n    UpdateStatisticsList(state, val) {\n      state.statisticsList = val;\n    },\n    UpdateBuyerStatisticsList(state, val) {\n      state.buyerStatisticsList = val;\n    },\n    UpdateReportStatistics(state, val) {\n      state.reportStatistics = val;\n    },\n    UpdateStatisticsResult(state, val) {\n      state.statisticsResult = val;\n    },\n    UpdatedepositWithdrawResult(state, val) {\n      state.depositWithdrawResult = val;\n    },\n    UpdatedepositWithdrawStatistics(state, val) {\n      state.depositWithdrawStatistics = val;\n    },\n    AddSumDepositWithdrawStatistics(state, val) {\n      state.depositWithdrawStatistics.push(val);\n    },\n    setStatTableData(stat, val) {\n      state.statTableData = val;\n    }\n  },\n  actions:{\n    // 获取就餐统计数据\n    statisticsHome({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsHome(data).then((res) => {\n          commit('UpdateStatHome', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐预约统计列表\n    statisticsAppointment({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsAppointment(data).then((res) => {\n          commit('UpdateStatAppointment', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 就餐统计导出\n    statisticsAppointmentExport({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsAppointmentExport(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐订单统计列表\n    statisticsOrder({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsOrder(data).then((res) => {\n          commit('UpdateStatOrder', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 就餐统计导出\n    statisticsOrderExport({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsOrderExport(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 获取部门就餐预约统计列表\n    statisticsDepAppointment({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsDepAppointment(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false)\n              reject(err);\n            });\n      });\n    },\n\n    // 获取部门就餐预约统计列表\n    statisticsDepOrder({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsDepOrder(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false)\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐统计数据\n    statisticsList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsList(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          commit('UpdateStatisticsList', res.data)\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false)\n            reject(err);\n          });\n      });\n    },\n    // PORTAL端交易统计\n    statisticsTradeList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsTradeList(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          commit('UpdateBuyerStatisticsList', res.data)\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n    // PORTAL端销售统计\n    statisticsSale({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsSale(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          // commit('UpdateStatisticsResult', res.data);\n          return resolve(res.data);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 就餐统计导出\n    statisticsDinnerExport({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsDinnerExport(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 交易统计导出\n    statisticsTradeExport({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsTradeExport(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 当日充值及提现统计\n    depositWithdrawToday({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.depositWithdrawToday(data).then((res) => {\n          let tmp = [];\n          tmp.push(res.data);\n          commit('UpdatedepositWithdrawStatistics', tmp);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 当日充值及提现统计导出\n    exportDepositWithdrawToday({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.exportDepositWithdrawToday(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 历史充值及提现统计\n    depositWithdraw({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.depositWithdraw(data).then((res) => {\n          commit('UpdatedepositWithdrawResult', res.data);\n          commit('UpdatedepositWithdrawStatistics', res.data.result);\n\n          let dataItem = {\n            date: '总计',\n            count: res.data.count,\n            amount: res.data.amount,\n            deposit_count: res.data.deposit_count,\n            deposit_amount: res.data.deposit_amount,\n            withdraw_count: res.data.withdraw_count,\n            withdraw_amount: res.data.withdraw_amount,\n            cancel_count: res.data.cancel_count,\n            cancel_amount: res.data.cancel_amount,\n          };\n\n          commit(\"AddSumDepositWithdrawStatistics\", dataItem);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 历史充值及提现统计导出\n    exportDepositWithdraw({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.exportDepositWithdraw(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n  }\n}\n"]}]}