entResetPwdStep(val){\n      switch(val){\n        case -1:\n        this.titleText = '请输入您需要重置登录密码的账户名';\n        break;\n        case 0:\n        this.titleText = '您正在进行身份验证';\n        break;\n        case 1:\n        this.titleText = '重置密码';\n        break;\n        case 2:\n        this.titleText = '重置完成';\n        break;\n      }  \n    }\n  }\n\n  \n}\n\n</script>\n<style lang='less' scoped>\n@import './style';\n</style>"],"sourceRoot":"src/views/common/forget-password"}]}