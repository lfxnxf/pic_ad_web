CAAA,gBAAA;AAEA;AAlBA;AA5BA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div>\n    <Table border stripe :columns='columns' :data='foodList.result' :loading=\"loading\">   \n      <template slot=\"description\" slot-scope=\"{ row }\">\n        <div v-html='row.desc'></div>\n      </template>\n      <template slot=\"status\" slot-scope=\"{ row }\">\n        <Tag :color=\"row.is_enable==1?'green':'volcano'\">{{row.is_enable==1?'正常':'下架'}}</Tag>\n      </template>\n      <template slot=\"handle\" slot-scope=\"{ row }\">\n        <Button type=\"primary\" ghost size=\"small\" icon=\"md-create\" style=\"border:0;\" @click=\"handleEdit(row)\">查看详情</Button>\n      </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport * as dataConfig from '../dataConfig';\nexport default {\n  name: 'SearchTable',\n  // props: {\n  //   value: Array\n  // },\n  data() {\n    return {\n      columns: [],\n\n    };\n  },\n\n  components: {},\n\n  computed: {\n    foodList() {\n      return this.$store.state.food.foodList;\n    },\n    loading() {\n      return this.$store.state.food.foodListLoading;\n    },\n   \n  },\n\n  mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n    handleEdit(row){\n      let data = {\n        food_id: row.food_id,\n        food_name: row.food_name,\n        desc: row.desc,\n        is_enable: row.is_enable.toString(),\n        img: row.img,\n        create_time: row.create_time,\n        update_time: row.update_time\n      }\n      this.$store.commit('setFoodDetailsModal', true);\n      this.$store.commit('setFoodDetail', data);\n      console.log(this.$store.state.food.foodDetailsModal);\n\n    }\n  }\n}\n</script>\n<style lang='less' scoped>\n</style>"],"sourceRoot":"src/views/setting/food-management/components"}]}