请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n// 获取商户列表\nexport const merchantList = (params) => {\n  const apiMethod = 'ih.canteen.merchant.list' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n\n// 同步用户信息，从云门户同步到用户中心\nexport const icloudSync = (params) => {\n  const apiMethod = 'ih.user.company.icloudSync' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n\n// 商户收银端账户信息列表\nexport const merchantUserList = (params) => {\n  const apiMethod = 'ih.canteen.user.list' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n// 添加商户收银端账户信息\nexport const merchantUserAdd = (params) => {\n  const apiMethod = 'ih.canteen.user.add' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n// 编辑商户收银端账户信息\nexport const merchantUserEdit = (params) => {\n  const apiMethod = 'ih.canteen.user.edit' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n// 商户收银端账户信息详情\nexport const merchantUserDetail = (params) => {\n  const apiMethod = 'ih.canteen.user.detail' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    method: 'post'\n  })\n}\n\n"]}]}