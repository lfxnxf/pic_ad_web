,cAAA,GAAA,IAAA;;AACA,UAAA,MAAA,GAAA,CAAA,CAAA,IAAA,CAAA,KAAA,WAAA,EAAA,UAAA,GAAA,EAAA;AACA,eAAA,GAAA,CAAA,QAAA,IAAA,GAAA,CAAA,KAAA;AACA,OAFA,CAAA;;AAGA,WAAA,IAAA,CAAA,QAAA,GAAA,MAAA,CAAA,MAAA;AACA,WAAA,aAAA,GAAA,MAAA;AACA,KAtDA;AAuDA;AACA,IAAA,WAxDA,yBAwDA;AACA,WAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,EAAA;AACA,WAAA,cAAA,GAAA,KAAA;AACA;AA7DA,GA/CA;AA+GA,EAAA,KAAA,EAAA;AACA,IAAA,SADA,qBACA,GADA,EACA;AACA,UAAA,GAAA,EAAA;AACA,aAAA,IAAA,CAAA,UAAA,GAAA,KAAA,WAAA,CAAA,OAAA;AACA,aAAA,iBAAA;AACA,aAAA,IAAA,qBAAA,KAAA,WAAA;AACA,aAAA,IAAA,CAAA,OAAA,GAAA,KAAA,WAAA,CAAA,UAAA;AACA,aAAA,aAAA,GAAA,KAAA,WAAA;AAEA;AACA;AAVA;AA/GA,CAAA","sourcesContent":["<!-- 修改商户信息 -->\n<template>\n  <Modal\n    v-model=\"editModal\"\n    title=\"编辑商户信息\"\n    :footer-hide=\"true\"\n    width=\"540px\">\n    <Form :model=\"form\" :label-width=\"120\" ref=\"editModal\" :rules=\"ruleForm\">\n      <FormItem label=\"商户ID\">\n        <Input v-model=\"form.merchant_id\" style=\"width: 340px\" placeholder=\"请输入商户ID\" :disabled=\"true\"/>\n      </FormItem>\n      <FormItem label=\"商户名称\" prop=\"merchant_name\">\n        <Input v-model=\"form.merchant_name\" style=\"width: 340px\" placeholder=\"请输入商户名称\" />\n      </FormItem>\n      <Divider>设置管理员，请先查找管理员，再下拉选择管理员信息</Divider>\n      <FormItem label=\"管理员查询\">\n        <Input v-model=\"form.admin_name\" style=\"width: 280px\" placeholder=\"按管理员姓名、手机号查找\" />\n        <Button type=\"primary\" @click=\"queryEmployeeList\">查询</Button>\n      </FormItem>\n      <FormItem label=\"选择管理员\">\n        <Select\n          v-model=\"form.contact\"\n          filterable\n          clearable\n          placeholder=\"选择商户管理员\"\n          @on-select=\"selectedContact\"\n          @on-clear=\"handleClear\"\n          style=\"width: 340px\">\n          <Option v-for=\"item of contactList\" :value=\"item.usercode\" :key=\"item.id\">\n            {{ `${item.name} [ ${item.department_name?item.department_name:''}-${item.phones?item.phones:''} ]` }}\n          </Option>\n        </Select>\n      </FormItem>\n      <FormItem label=\"管理员账号\" prop=\"admin_user\">\n        <Input v-model=\"form.admin_user\" style=\"width: 340px\" :disabled=\"true\" placeholder=\"管理员账号（选择管理员，自动填写）\" />\n      </FormItem>\n      <FormItem label=\"联系电话\">\n        <Input v-model=\"form.link_tel\" style=\"width: 340px\" placeholder=\"联系人电话\" />\n      </FormItem>\n      <FormItem label=\"是否有效\">\n        <RadioGroup v-model=\"form.is_enable\">\n          <Radio label=\"1\">有效</Radio>\n          <Radio label=\"0\">无效</Radio>\n        </RadioGroup>\n      </FormItem>\n      <div class=\"text-center margin-bottom-20 margin-top-30\">\n        <Button class=\"margin-right-20\" @click=\"$store.commit('setEditMerchantModal', false)\">取消</Button>\n        <Button type=\"primary\" :loading=\"btnLoading\" @click=\"editMerchant\">确定</Button>\n      </div>\n    </Form>\n  </Modal>\n</template>\n\n<script>\nimport _ from \"underscore\";\nimport {localRead} from \"@/libs/util\";\nexport default {\n  data () {\n    return {\n      btnLoading: false,\n      showContact: true,\n      form: {\n        merchant_name: '',\n        payment_type: 'PB_SMT',\n        admin_user: '',\n        contact: '',\n        link_tel: '',\n        is_enable: ''\n      },\n      ruleForm: {\n        merchant_name: [\n          { required: true, message: '请输入商户名称', trigger: 'blur' }\n        ],\n        admin_user: [\n          { required: true, message: '请选择管理员', trigger: 'blur' }\n        ],\n      },\n      selectContact: {},\n      contactList: [],\n      input_disabled: true\n    };\n  },\n\n  components: {},\n\n  computed: {\n    editModal:{\n      get(){\n        return this.$store.state.merchant.editMerchantModal;\n      },\n      set(val){\n        this.$store.commit('setEditMerchantModal', val)\n      }\n    },\n    currentData(){\n      return this.$store.state.merchant.currentData;\n    },\n    getOrgId(){\n      return this.$store.state.user.curOrgId;\n    }\n\n  },\n\n  methods: {\n    editMerchant: async function(){\n      this.$refs.editModal.validate((valid) => {\n        if (valid) {\n          this.btnLoading = true;\n          let params = {\n            ...this.form,\n            org_id: this.getOrgId,\n          }\n          params.contact = this.selectContact.contact ? this.currentData.contact : this.selectContact.name;\n          console.log('params', params)\n          this.$store.dispatch('merchantEdit', params).then(res => {\n            if (res.return_code == 0) {\n              this.btnLoading = false;\n              this.$Message.success('修改商户信息成功！')\n              this.$store.dispatch('merchantList', {org_id: this.getOrgId, page_size: this.$parent.page_size});\n              this.$store.commit('setEditMerchantModal', false);\n              this.$refs.editModal.resetFields();\n            }\n          }).catch(err=>{\n            this.btnLoading = false;\n          });\n        }\n      });\n    },\n    //查询企业员工信息\n    queryEmployeeList: async function() {\n      let adminname = this.form.admin_name || '';\n      if (adminname.length == 0){\n        this.$Message.error('请输入管理员信息！');\n        return;\n      }\n\n      let params = {\n        sessionid: this.$store.state.user.token,\n        org_id: this.$store.state.user.curOrgId,\n        usercode: this.$store.state.user.empUserCode,\n        query: this.form.admin_name,\n        status: 1,\n        limit: 1000\n      };\n      let res = await this.$store.dispatch('employeeList', params);\n      this.contactList = res.rows;\n    },\n\n    //选中联系人后\n    selectedContact(val){\n      this.form.admin_user = val.value;\n      this.input_disabled = true;\n      let result = _.find(this.contactList, cur=>{\n        return cur.usercode == val.value;\n      })\n      this.form.link_tel = result.phones;\n      this.selectContact = result;\n    },\n    //联系人点击清空\n    handleClear(){\n      this.form.admin_user = '';\n      this.form.link_tel = '';\n      this.form.contact = '';\n      this.input_disabled = false;\n    }\n\n  },\n  watch:{\n    editModal(val){\n      if(val){\n        this.form.admin_name = this.currentData.contact;\n        this.queryEmployeeList();\n        this.form = {...this.currentData};\n        this.form.contact = this.currentData.admin_user\n        this.selectContact = this.currentData;\n\n      }\n    }\n  },\n\n}\n\n</script>\n<style lang='less' scoped>\n</style>\n"],"sourceRoot":"src/views/business-information/basic-information/components"}]}