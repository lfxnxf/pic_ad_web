olor(val) {\n      let color = '';\n      switch(val){\n        case '0':\n          color = \"default\";\n          break;\n        case '1':\n          color = \"volcano\";\n          break;\n        case '2':\n          color = \"green\";\n          break;\n        case '3':\n          color = \"blue\";\n      }\n      return color;\n    }\n  }\n}\n</script>\n<style lang='less' scoped>\n    /deep/.ivu-table td.order_amount{\n        color: #ff602e;\n    }\n</style>"],"sourceRoot":"src/views/reserve/components"}]}