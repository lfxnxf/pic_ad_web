AA;AAAA,SARA,EASA;AAAA,qBAAA,EAAA;AAAA,kBAAA;AAAA,SATA,EAUA;AAAA,qBAAA,EAAA;AAAA,kBAAA;AAAA,SAVA,EAWA;AAAA,qBAAA,EAAA;AAAA,kBAAA;AAAA,SAXA,CAAA;AAYA,YAAA,GAAA,GAAA,EAAA;;AACA,YAAA,OAAA,GAAA,CAAA,CAAA,IAAA,CAAA,cAAA,EAAA,UAAA,GAAA,EAAA;AACA,iBAAA,GAAA,CAAA,OAAA,IAAA,EAAA;AACA,SAFA,CAAA;;AAGA,QAAA,GAAA,GAAA,OAAA,GAAA,OAAA,CAAA,IAAA,GAAA,EAAA;AACA,eAAA,GAAA;AACA,OAnBA;AAoBA;AAtBA,GAtBA;AA+CA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,OAAA,GAAA,UAAA,CAAA,WAAA;AACA,KAHA;AAKA;AACA,IAAA,wBAAA;AAAA,8FAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA;AACA,kBAAA,SAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KADA;AAEA,kBAAA,QAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA,QAFA;AAGA,kBAAA,aAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAHA;AAIA,kBAAA,UAAA,EAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAJA;AAKA,kBAAA,QAAA,EAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA,YAAA;AALA,iBADA,EAQA;;AARA;AAAA,uBASA,KAAA,MAAA,CAAA,QAAA,CAAA,qBAAA,EAAA,MAAA,WAAA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBAAA,CATA;;AAAA;AASA,gBAAA,GATA;;AAUA;AACA,oBAAA,GAAA,CAAA,WAAA,IAAA,CAAA,MAAA,EAAA;AACA,uBAAA,QAAA,CAAA,KAAA,CAAA;AACA,oBAAA,OAAA,EAAA,eADA;AAEA,oBAAA,QAAA,EAAA,CAFA;AAGA,oBAAA,QAAA,EAAA;AAHA,mBAAA;AAKA,uBAAA,OAAA,CAAA,OAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA;AACA;;AACA,qBAAA,SAAA,GAAA,GAAA,CAAA,IAAA;;AAnBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AANA,GA/CA;AA8EA,EAAA,OA9EA,qBA8EA;AACA,SAAA,IAAA;AACA,SAAA,wBAAA;AACA;AAjFA,CAAA","sourcesContent":["<!-- 钱包充值 -->\n<template>\n  <div class=\"pre_paid card\" style=\"min-height: 50vh\">\n    <div>\n      <h3 class=\"margin-bottom-15\">交易明细</h3>\n      <Form v-model=\"formSearch\" inline>\n        <FormItem label=\"交易时间\">\n          <DatePicker v-model=\"date\" type=\"daterange\" :clearable=\"false\" :editable=\"false\" placeholder=\"交易时间起止\" style=\"width: 200px\"></DatePicker>\n        </FormItem>\n        <FormItem>\n          <Button type='primary' icon=\"md-search\" @click=\"queryUserWalletTradeLogs()\">查询</Button>\n        </FormItem>\n      </Form>\n      <Table border stripe :columns='columns' :data='tableData.data' :loading=\"loading\">\n<!--        <template slot=\"tradetype\" slot-scope=\"{ row }\">-->\n<!--          <span>{{ tradetype(row.trade_type) }}</span>-->\n<!--        </template>-->\n        <template slot=\"amount\" slot-scope=\"{ row }\">\n          <p class=\"orange-color\">￥{{(row.money/100).toFixed(2)}}</p>\n        </template>\n        <template slot=\"curbalance\" slot-scope=\"{ row }\">\n          <p class=\"orange-color\">￥{{(row.balance/100).toFixed(2)}}</p>\n        </template>\n      </Table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as dataConfig from '../dataConfig';\nimport _ from 'underscore';\nimport moment from \"moment-timezone\";\nexport default {\n  name: 'PrePaid',\n  props: {\n    routeParams: Object\n  },\n  data () {\n    return {\n      date: [moment().format('YYYY-MM-DD'), moment().format('YYYY-MM-DD')],\n      balance: 0,\n      columns: [],\n      tableData: [],\n      loading: false,\n      form: {\n        amount: '',\n        remark: ''\n      },\n      formSearch: {},\n    };\n  },\n\n  components: {},\n\n  computed: {\n    tradetype() {\n      return (id)=>{\n        let tradetypeArray=[{\"tradeid\":0,\"desc\":\"消费\"},\n          {\"tradeid\":1,\"desc\":\"充值\"},\n          {\"tradeid\":2,\"desc\":\"提现\"},\n          {\"tradeid\":3,\"desc\":\"结算\"},\n          {\"tradeid\":4,\"desc\":\"返现\"},\n          {\"tradeid\":5,\"desc\":\"手续\"},\n          {\"tradeid\":7,\"desc\":\"对账\"},\n          {\"tradeid\":8,\"desc\":\"撤销\"},\n          {\"tradeid\":9,\"desc\":\"退货\"},\n          {\"tradeid\":12,\"desc\":\"预退款请求\"},\n          {\"tradeid\":13,\"desc\":\"预退款撤销\"},\n          {\"tradeid\":21,\"desc\":\"积分\"}];\n        let val = '';\n        let current = _.find(tradetypeArray, cur=> {\n          return cur.tradeid == id;\n        });\n        val = current ? current.desc : '';\n        return val;\n      }\n    }\n  },\n\n  methods: {\n    init(){\n      this.columns = dataConfig.columnscash;\n    },\n\n    //查询员工钱包交易记录信息\n    queryUserWalletTradeLogs: async function() {\n      let params = {\n        sessionid: this.$store.state.user.token,\n        usercode: this.$route.params.usercode,\n        merchant_code: this.$store.state.user.curMerchantUserCode,\n        start_date: moment(this.date[0]).format('YYYY-MM-DD'),\n        end_date: moment(this.date[1]).format('YYYY-MM-DD'),\n      };\n      // console.log(\"queryUserWalletTradeLogs----->>>>\" + JSON.stringify(params));\n      let res = await this.$store.dispatch('userWalletTradeLogs', params).catch(err => err);\n      // console.log(\"员工钱包交易记录信息---res-->>>>\" + JSON.stringify(res));\n      if (res.return_code == -301003){\n        this.$Message.error({\n          content: \"登录会话超时，请重新登录！\",\n          duration: 8,\n          closable: true\n        });\n        this.$router.replace({path:'/login'});\n      }\n      this.tableData = res.data;\n    },\n\n\n  },\n\n  mounted(){\n    this.init();\n    this.queryUserWalletTradeLogs();\n  }\n}\n\n</script>\n\n<style lang='less' scoped>\n\n/deep/.ivu-table td.order_amount{\n  color: #ff602e;\n}\n/deep/.ivu-table-wrapper{\n  overflow: visible;\n}\n.refund{\n  /deep/.ivu-icon{\n    display: none;\n    left: 16px;\n  }\n}\n\n/deep/.ivu-poptip-title-inner{\n  color: #e42329;\n  padding: 10px;\n}\n</style>\n"],"sourceRoot":"src/views/staff-management/details/components"}]}