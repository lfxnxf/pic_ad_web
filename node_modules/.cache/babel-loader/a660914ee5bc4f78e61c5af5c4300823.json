-->\n<template>\n  <div class=\"card\">\n    <Table border stripe :columns='columns' :data='merchantList.result' :loading=\"tableLoading\">\n      <template slot=\"is_enable\" slot-scope=\"{ row }\">\n        <Tag :color=\"row.is_enable==1?'green':'volcano'\">{{row.is_enable==1?'有效':'无效'}}</Tag>\n      </template>\n      <template slot=\"handle\" slot-scope=\"{ row }\">\n        <Button type=\"primary\" ghost size=\"small\" icon=\"md-create\" style=\"border:0;\" @click=\"handleEdit(row)\">修改</Button>\n      </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport * as dataConfig from '../dataConfig';\nexport default {\n  name: 'SearchTable',\n  data() {\n    return {\n      columns: [],\n    };\n  },\n\n  components: {},\n\n  computed: {\n    merchantList() {\n      return this.$store.state.merchant.merchantList;\n    },\n    tableLoading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  async mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n\n    handleEdit(row){\n      let data = row;\n      data.is_enable = row.is_enable.toString();\n      \n      this.$store.commit('SetMerchantCurrentData', data);\n      this.$store.commit('setEditMerchantModal', true);\n      console.log(\"===>merchantEditModal：\" + this.$store.state.merchant.editMerchantModal + \"， data：\" + JSON.stringify(data));\n\n    },\n\n  },\n\n}\n</script>\n<style lang='less' scoped>\n\n</style>\n"],"sourceRoot":"src/views/business-information/basic-information/components"}]}