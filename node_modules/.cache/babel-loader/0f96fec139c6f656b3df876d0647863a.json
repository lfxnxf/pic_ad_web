ENTDATA', data);\n\n    },\n\n    //获取人员类型列表\n    queryRole: async function(page, page_size) {\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        is_enable: 1,\n        page: page || 1,\n        page_size: page_size || this.$parent.page_size\n      };\n      return await this.$store.dispatch('roleList', params);\n    },\n\n  },\n\n}\n</script>\n<style lang='less' scoped>\n\n</style>\n"],"sourceRoot":"src/views/setting/food-card-setting/components"}]}