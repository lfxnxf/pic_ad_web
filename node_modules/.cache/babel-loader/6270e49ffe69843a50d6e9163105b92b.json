n        })\n            .catch((err) => {\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐列表\n    goodsDateList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        goods_date.goodsDateList(data).then((res) => {\n            commit('UPDATEDOODSDATEDATA', res.data)\n            return resolve(res);\n          })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n\n    // 添加就餐信息\n    goodsDateEdit({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        goods_date.goodsDateEdit(data).then((res) => {\n          return resolve(res);\n        })\n            .catch((err) => {\n              reject(err);\n            });\n      });\n    },\n\n    // 批量添加或修改菜单信息\n     goodsDateEditNew({ commit }, params) {\n      return new Promise((resolve, reject) => {\n        goods_date.goodsDateEditNew(params).then((res) => {\n            commit('UPDATEDOODSDATEDATA', res.data)\n            return resolve(res);\n          })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n  },\n\n}\n"]}]}