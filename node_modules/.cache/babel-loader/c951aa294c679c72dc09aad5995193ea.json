=> {\n        goods.goodsAdd(data).then((res) => {\n            return resolve(res);\n          })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n    // 菜品日历\n    goodsCalendar({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        goods.goodsCalendar(data).then((res) => {\n          return resolve(res);\n        })\n            .catch((err) => {\n              reject(err);\n            });\n      });\n    },\n    // 菜品详情\n    goodsDetail({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        goods.goodsDetail(data).then((res) => {\n          commit('setGoodsDetail', res.data);\n          return resolve(res);\n        })\n            .catch((err) => {\n              reject(err);\n            });\n      });\n    },\n  }\n}\n"]}]}