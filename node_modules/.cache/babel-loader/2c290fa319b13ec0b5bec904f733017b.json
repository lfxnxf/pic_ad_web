,aAAO,IAAIC,OAAJ,CAAY,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACtChC,QAAAA,UAAU,CAAC2D,qBAAX,CAAiC/B,IAAjC,EAAuCK,IAAvC,CAA4C,UAACC,GAAD,EAAS;AACnDL,UAAAA,MAAM,CAAC,eAAD,EAAkB,KAAlB,CAAN;AACA,iBAAOE,OAAO,CAACG,GAAD,CAAd;AACD,SAHD,WAIS,UAACC,GAAD,EAAS;AACdN,UAAAA,MAAM,CAAC,eAAD,EAAkB,KAAlB,CAAN;AACAG,UAAAA,MAAM,CAACG,GAAD,CAAN;AACD,SAPH;AAQD,OATM,CAAP;AAUD;AA1NK;AA/CK,CAAf","sourcesContent":["\nimport * as statistics from '@/api/statistics';\nexport default {\n  state:{\n    statisticsList: {},\n    buyerStatisticsList: {},\n    statisticsResult: {},\n    reportStatistics: [],\n    statisticsListLoading: false,\n    depositWithdrawResult: {},\n    depositWithdrawStatistics: [],\n    statHome: {},\n    statAppointment: {},\n    statOrder: {},\n  },\n  getters: {\n  },\n  mutations:{\n    UpdateStatHome(state, val) {\n      state.statHome = val;\n    },\n    UpdateStatAppointment(state, val) {\n      state.statAppointment = val;\n    },\n    UpdateStatOrder(state, val) {\n      state.statOrder = val;\n    },\n    UpdateStatisticsList(state, val) {\n      state.statisticsList = val;\n    },\n    UpdateBuyerStatisticsList(state, val) {\n      state.buyerStatisticsList = val;\n    },\n    UpdateReportStatistics(state, val) {\n      state.reportStatistics = val;\n    },\n    UpdateStatisticsResult(state, val) {\n      state.statisticsResult = val;\n    },\n    UpdatedepositWithdrawResult(state, val) {\n      state.depositWithdrawResult = val;\n    },\n    UpdatedepositWithdrawStatistics(state, val) {\n      state.depositWithdrawStatistics = val;\n    },\n    AddSumDepositWithdrawStatistics(state, val) {\n      state.depositWithdrawStatistics.push(val);\n    },\n  },\n  actions:{\n    // 获取就餐统计数据\n    statisticsHome({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsHome(data).then((res) => {\n          commit('UpdateStatHome', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐预约统计列表\n    statisticsAppointment({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsAppointment(data).then((res) => {\n          commit('UpdateStatAppointment', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐订单统计列表\n    statisticsOrder({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsOrder(data).then((res) => {\n          commit('UpdateStatOrder', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false);\n              reject(err);\n            });\n      });\n    },\n\n    // 获取部门就餐预约统计列表\n    statisticsDepAppointment({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsDepAppointment(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false)\n              reject(err);\n            });\n      });\n    },\n\n    // 获取部门就餐预约统计列表\n    statisticsDepOrder({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsDepOrder(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          return resolve(res);\n        })\n            .catch((err) => {\n              commit('TABLE_LOADING', false)\n              reject(err);\n            });\n      });\n    },\n\n    // 获取就餐统计数据\n    statisticsList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsList(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          commit('UpdateStatisticsList', res.data)\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false)\n            reject(err);\n          });\n      });\n    },\n    // PORTAL端交易统计\n    statisticsTradeList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsTradeList(data).then((res) => {\n          commit('TABLE_LOADING', false)\n          commit('UpdateBuyerStatisticsList', res.data)\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n    // PORTAL端销售统计\n    statisticsSale({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsSale(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          // commit('UpdateStatisticsResult', res.data);\n          return resolve(res.data);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 就餐统计导出\n    statisticsDinnerExport({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsDinnerExport(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n    // 交易统计导出\n    statisticsTradeExport({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        statistics.statisticsTradeExport(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n          });\n      });\n    },\n\n 