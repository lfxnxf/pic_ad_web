se,\n    addMerchantModal: false,\n    editMerchantModal: false,\n    currentData: {},\n    merchantData: {},\n  },\n  getters: {\n  },\n  mutations:{\n    UpdateMerchantList(state, val) {\n      state.merchantList = val;\n    },\n    SetMerchantCurrentData(state, val){\n      state.currentData = val;\n    },\n    setEditMerchantModal(state, val){\n      state.editMerchantModal = val\n    },\n    setAddMerchantModal(state, val){\n      state.addMerchantModal = val\n    },\n    UPDATEMERCHANTDATA(state, val) {\n      state.merchantData = val;\n    },\n  },\n  actions:{\n    // 获取商户详情\n    merchantDetail({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantDetail(data).then((res) => {\n          commit('UPDATEMERCHANTDATA', res.data)\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n    merchantEdit({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantEdit(data).then((res) => {\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n    // 获取商户列表\n    merchantList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantList(data).then((res) => {\n          commit('UpdateMerchantList', res.data)\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n\n          });\n      });\n    },\n    // 添加商户信息\n    merchantAdd({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantAdd(data).then((res) => {\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n\n    icloudSync({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        