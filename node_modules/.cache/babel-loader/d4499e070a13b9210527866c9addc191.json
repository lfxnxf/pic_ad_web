KAAA,CAAA,GAAA,CAAA,YAAA;AACA;AAdA,GAnBA;AAoCA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,OAAA,GAAA,UAAA,CAAA,OAAA;AACA,KAHA;AAKA;AACA,IAAA,KANA,iBAMA,IANA,EAMA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,eAAA,EAAA,IAAA;AACA,UAAA,SAAA,GAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;AACA,UAAA,OAAA,GAAA,MAAA,CAAA,KAAA,IAAA,CAAA,CAAA,CAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;;AACA,UAAA,SAAA,IAAA,EAAA,IAAA,SAAA,IAAA,cAAA,IAAA,OAAA,IAAA,EAAA,IAAA,OAAA,IAAA,cAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,MAAA;AACA,QAAA,WAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aADA;AAEA,QAAA,IAAA,EAAA,IAAA,IAAA,CAFA;AAGA,QAAA,SAAA,EAAA,KAAA;AAHA,SAIA,KAAA,UAJA,CAAA;;AAOA,MAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,EAfA,CAiBA;AACA;AACA;AACA;AACA;AACA;AACA,KA7BA;AA+BA,IAAA,cA/BA,0BA+BA,OA/BA,EA+BA;AACA,WAAA,KAAA,CAAA,OAAA;AACA;AAjCA,GApCA;AAwEA,EAAA,OAxEA,qBAwEA;AACA,SAAA,IAAA;AACA,SAAA,KAAA;AACA;AA3EA,CAAA","sourcesContent":["<!-- 钱包充值 -->\n<template>\n  <div class=\"pre_paid card\" style=\"min-height: 50vh\">\n    <div>\n      <h3 class=\"margin-bottom-15\">交易明细</h3>\n      <Form v-model=\"formSearch\" inline>\n        <FormItem label=\"交易时间\">\n          <DatePicker v-model=\"date\" type=\"daterange\" :clearable=\"false\" :editable=\"false\" placeholder=\"交易时间起止\" style=\"width: 200px\"></DatePicker>\n        </FormItem>\n        <FormItem>\n          <Button type='primary' icon=\"md-search\" @click=\"query()\">查询</Button>\n        </FormItem>\n      </Form>\n      <Table border stripe :columns='columns' :data='tableData' :loading=\"loading\">\n        <template slot=\"trade_type\" slot-scope=\"{ row }\">\n          <span>{{ row.trade_type_desc }}</span>\n        </template>\n        <template slot=\"amount\" slot-scope=\"{ row }\">\n          <p class=\"orange-color\"><span v-show=\"row.trade_type==3||row.trade_type==4\">-</span>￥{{row.amount}}</p>\n        </template>\n        <template slot=\"buyer\" slot-scope=\"{ row }\">\n          <span>{{row.usercode?`[${row.usercode}]`:''}}</span>\n          <span> {{row.name?`${row.name}`:''}}</span>\n        </template>\n\n      </Table>\n      <Page class=\"page\" ref=\"pages\" :total=\"total\" :page-size=\"page_size\" size=\"small\" show-total @on-change=\"handlePageDown\"></Page>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as dataConfig from '../dataConfig';\nimport _ from 'underscore';\nimport moment from \"moment-timezone\";\nimport {buildChildDepartmentArray} from \"@/libs/util\";\nexport default {\n  name: 'PrePaid',\n  props: {\n    routeParams: Object\n  },\n  data () {\n    return {\n      date: [moment().format('YYYY-MM-DD'), moment().format('YYYY-MM-DD')],\n      balance: 0,\n      columns: [],\n      tableData: [],\n      formSearch: {},\n      page_size: 50,\n      total: 0,\n    };\n  },\n\n  components: {},\n\n  computed: {\n    resultForm(){\n      let data = {\n        start_date: moment(this.date[0]).format('YYYY-MM-DD'),\n        end_date: moment(this.date[1]).format('YYYY-MM-DD'),\n        trade_types: '',\n        departments: '',\n        role_ids: '',\n        users: this.$route.params.usercode,\n      }\n      return data;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n  },\n\n  methods: {\n    init(){\n      this.columns = dataConfig.columns;\n    },\n\n    //查询员工钱包交易记录信息\n    query(page) {\n      this.$store.commit('TABLE_LOADING', true);\n      let startDate = moment(this.date[0]).format('YYYY-MM-DD');\n      let endDate = moment(this.date[1]).format('YYYY-MM-DD');\n      if (startDate == '' || startDate == 'Invalid date' || endDate == '' || endDate == 'Invalid date'){\n        this.$Message.error(\"请选择交易日期起止！\");\n        return;\n      }\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        page: page || 1,\n        page_size: this.page_size,\n        ...this.resultForm\n      };\n\n      console.log(\"query--->>>\" + JSON.stringify(params))\n\n      // this.$store.dispatch('tradeUserList', params).then(res=>{\n      //   if (res.return_code == 0){\n      //     this.tableData = res.data.result;\n      //     this.total = res.data.total;\n      //   }\n      // }).catch(err => {});\n    },\n\n    handlePageDown(curPage){\n      this.query(curPage);\n    }\n  },\n\n  mounted(){\n    this.init();\n    this.query();\n  }\n}\n\n</script>\n<style lang='less' scoped>\n\n/deep/.ivu-table td.order_amount{\n  color: #ff602e;\n}\n/deep/.ivu-table-wrapper{\n  overflow: visible;\n}\n.refund{\n  /deep/.ivu-icon{\n    display: none;\n    left: 16px;\n  }\n}\n\n/deep/.ivu-poptip-title-inner{\n  color: #e42329;\n  padding: 10px;\n}\n</style>\n"],"sourceRoot":"src/views/staff-management/details/components"}]}