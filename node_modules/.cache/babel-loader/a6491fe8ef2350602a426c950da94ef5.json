kBAAA,SAAA,EAAA;AAJA,iBADA;AAAA;AAAA,uBAOA,KAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,MAAA,CAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAdA;AAwBA,IAAA,cAxBA,0BAwBA,OAxBA,EAwBA;AACA,WAAA,KAAA,CAAA,SAAA,CAAA,SAAA,CAAA,OAAA;AACA,KA1BA;AA2BA,IAAA,OA3BA,mBA2BA,GA3BA,EA2BA;AACA,WAAA,UAAA,GAAA,GAAA,CAAA,KAAA;AACA,WAAA,QAAA,GAAA,GAAA,CAAA,GAAA;AACA;AA9BA,GAvBA;AAyDA,EAAA,OAzDA,qBAyDA;AACA,SAAA,aAAA;AACA,SAAA,UAAA;AAEA,GA7DA;AA8DA,EAAA,SA9DA,uBA8DA;AACA,SAAA,KAAA,CAAA,SAAA,CAAA,SAAA;AACA;AAhEA,CAAA","sourcesContent":["<!-- 就餐统计 -->\n<template>\n  <div>\n    <!-- <h3 class=\"margin-bottom-10\">就餐列表</h3> -->\n    <div class=\"margin-bottom-10\">\n      <searchForm ref=\"FormQuery\" @getDate=\"getDate\"></searchForm>\n    </div>\n    <div>\n      <searchTable :startDate=\"start_date\" :endDate=\"end_date\"></searchTable>\n    </div>\n    <Page class=\"page\" ref=\"pages\" :total=\"statisticsList.total\" :page-size=\"page_size\" size=\"small\" show-total @on-change=\"handlePageDown\"></Page>\n  </div>\n</template>\n\n<script>\nimport searchForm from './components/form';\nimport searchTable from './components/table';\nexport default {\n  name: 'DiningStat',\n  data () {\n    return {\n      page_size: 50,\n      start_date: '',\n      end_date: ''\n    };\n  },\n\n  components: {\n    searchForm, searchTable,\n  },\n\n  computed: {\n    statisticsList() {\n      return this.$store.state.statistics.statAppointment;\n    },\n    currentMerchantId(){\n      return this.$store.state.user.curMerchantId;\n    },\n  },\n\n  methods: {\n    //查询角色\n    queryRoleList: async function() {\n      let params = {\n        merchant_id: this.currentMerchantId,\n        is_enable: 1,\n        page: 1,\n        page_size: 10000\n      };\n      await this.$store.dispatch('roleList', params);\n\n    },\n\n    // 获取餐种list\n    queryGoods: async function() {\n      let params = {\n        merchant_id: this.currentMerchantId,\n        page: 1,\n        page_size: 100,\n        is_enable: 1\n      };\n      await this.$store.dispatch('goodsList', params);\n\n    },\n    handlePageDown(curPage){\n      this.$refs.FormQuery.queryStat(curPage);\n    },\n    getDate(val){\n      this.start_date = val.start;\n      this.end_date = val.end;\n    },\n\n  },\n\n  mounted(){\n    this.queryRoleList();\n    this.queryGoods();\n\n  },\n  activated(){\n    this.$refs.FormQuery.queryStat();\n  }\n}\n\n</script>\n<style lang='less' scoped>\n</style>\n"],"sourceRoot":"src/views/statistics/stat-appointment"}]}