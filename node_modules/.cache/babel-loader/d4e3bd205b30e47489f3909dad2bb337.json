rd\">\n    <Table border stripe :columns='columns' :data='goodsList.result' :loading=\"loading\">\n      <template slot=\"is_enable\" slot-scope=\"{ row }\">\n        <Tag :color=\"row.is_enable==1?'green':'volcano'\">{{row.is_enable==1?'有效':'无效'}}</Tag>\n      </template>\n      <template slot=\"handle\" slot-scope=\"{ row }\">\n        <Button type=\"primary\" ghost size=\"small\" icon=\"md-create\" style=\"border:0;\" @click=\"handleEdit(row)\">修改</Button>\n      </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport * as dataConfig from '../dataConfig';\nexport default {\n  name: 'SearchTable',\n  data() {\n    return {\n      columns: [],\n    };\n  },\n\n  components: {},\n\n  computed: {\n    goodsList() {\n      return this.$store.state.goods.goodsList;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n    handleEdit(row){\n      let data = {\n        goods_id: row.goods_id,\n        goods_name: row.goods_name,\n        description: row.description,\n        is_enable: row.is_enable.toString(),\n        start_time: row.start_time,\n        end_time: row.end_time,\n      }\n      this.$store.commit('setEditGoodsModal', true);\n      this.$store.commit('setCurrentGoodsData', data);\n      console.log(this.$store.state.goods.editGoodsModal);\n\n    }\n  },\n\n}\n</script>\n<style lang='less' scoped>\n\n</style>\n"],"sourceRoot":"src/views/setting/time-type-setting/components"}]}