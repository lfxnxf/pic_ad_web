AA,KAAA,EAAA;AACA,oBAAA,KAAA,CAAA,UAAA,GAAA,IAAA;AACA,wBAAA,MAAA,GAAA;AACA,sBAAA,SAAA,EAAA,KAAA,CAAA,WAAA,CAAA,SADA;AAEA,sBAAA,WAAA,EAAA,KAAA,CAAA,IAAA,CAAA,WAFA;AAGA,sBAAA,IAAA,EAAA,KAAA,CAAA,IAAA,CAAA,IAHA;AAIA,sBAAA,SAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAJA,qBAAA;;AAMA,oBAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,0BAAA,GAAA,CAAA,WAAA,IAAA,CAAA,EAAA;AACA,wBAAA,KAAA,CAAA,UAAA,GAAA,KAAA;;AACA,wBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,wBAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA;AACA,0BAAA,WAAA,EAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aADA;AAEA,0BAAA,SAAA,EAAA,KAAA,CAAA,OAAA,CAAA;AAFA,yBAAA;;AAIA,wBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,KAAA;;AACA,wBAAA,KAAA,CAAA,KAAA,CAAA,SAAA,CAAA,WAAA;AACA;AACA,qBAXA,WAWA,UAAA,GAAA,EAAA;AACA,sBAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA,qBAbA;AAcA;AACA,iBAxBA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AADA,GAtCA;AAmEA,EAAA,KAAA,EAAA;AACA,IAAA,SADA,qBACA,GADA,EACA;AACA,UAAA,GAAA,EAAA;AACA,aAAA,IAAA,GAAA,KAAA,WAAA;AACA,OAFA,MAEA;AACA,aAAA,UAAA,GAAA,KAAA;AACA;AACA;AAPA;AAnEA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <Modal\n    v-model=\"editModal\"\n    title=\"修改设备信息\"\n    :footer-hide=\"true\"\n    width=\"420px\">\n    <Form :model=\"form\" :label-width=\"90\" ref=\"editModal\" :rules=\"ruleForm\">\n      <FormItem label=\"设备号\"  prop=\"device_code\">\n        <Input v-model=\"form.device_code\" style=\"width: 200px\" placeholder=\"请输入设备号\" />\n      </FormItem>\n      <FormItem label=\"设备名称\"  prop=\"name\">\n        <Input v-model=\"form.name\" style=\"width: 200px\" placeholder=\"请输入设备名称\" />\n      </FormItem>\n      <FormItem label=\"是否有效\">\n        <RadioGroup v-model=\"form.is_enable\">\n          <Radio label=\"1\">有效</Radio>\n          <Radio label=\"0\">无效</Radio>\n        </RadioGroup>\n      </FormItem>\n      <div class=\"text-center margin-bottom-20 margin-top-30\">\n        <Button class=\"margin-right-20\" @click=\"$store.commit('setEditDeviceModal', false)\">取消</Button>\n        <Button type=\"primary\" :loading=\"btnLoading\" @click=\"editDevice\">确定</Button>\n      </div>\n    </Form>\n  </Modal>\n</template>\n\n<script>\nimport _ from \"underscore\";\nexport default {\n  data () {\n    return {\n      btnLoading: false,\n      form: {\n        device_id: '',\n        device_code: '',\n        name: '',\n        is_enable: ''\n      },\n      ruleForm: {\n        device_code: [\n          { required: true, message: '请输入设备号', trigger: 'blur' }\n        ],\n        name: [\n          { required: true, message: '请输入设备名称', trigger: 'blur' }\n        ],\n      }\n    };\n  },\n\n  components: {},\n\n  computed: {\n    editModal:{\n      get(){\n        return this.$store.state.device.editDeviceModal;\n      },\n      set(val){\n        this.$store.commit('setEditDeviceModal', val)\n      }\n    },\n    currentData(){\n      return this.$store.state.device.currentData;\n    },\n\n  },\n\n  methods: {\n    editDevice: async function(){\n      this.$refs.editModal.validate((valid) => {\n        if (valid) {\n          this.btnLoading = true;\n          let params = {\n            device_id: this.currentData.device_id,\n            device_code: this.form.device_code,\n            name: this.form.name,\n            is_enable: this.form.is_enable\n          }\n          this.$store.dispatch('deviceEdit', params).then(res => {\n            if (res.return_code == 0) {\n              this.btnLoading = false;\n              this.$Message.success('修改成功！')\n              this.$store.dispatch('deviceList', {\n                merchant_id: this.$store.state.user.curMerchantId,\n                page_size: this.$parent.page_size\n              });\n              this.$store.commit('setEditDeviceModal', false);\n              this.$refs.editModal.resetFields();\n            }\n          }).catch(err=>{\n            this.btnLoading = false;\n          });\n        }\n      });\n    },\n  },\n  watch:{\n    editModal(val){\n      if(val){\n        this.form = this.currentData;\n      }else{\n        this.btnLoading = false;\n      }\n    }\n  }\n}\n\n</script>\n<style lang='less' scoped>\n</style>\n"],"sourceRoot":"src/views/setting/device-setting/components"}]}