...params, method: apiMethod }\n  return axios.request({\n    data,\n    // params: { method: apiMethod },\n    method: 'post'\n  })\n}\n\n\n// 导出就餐记录列表\nexport const exportDiningList = (params) => {\n  const apiMethod = 'ih.canteen.order.export' // 如果是通过网关请求，需要配置网关api，否则不用配置此字段\n  const data = { ...params, method: apiMethod }\n  return axios.request({\n    data,\n    // params: { method: apiMethod },\n    method: 'post'\n  })\n}\n\n"]}]}