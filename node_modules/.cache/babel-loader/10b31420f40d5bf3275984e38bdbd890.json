     })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n    // 添加设备\n    deviceAdd({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        device.deviceAdd(data).then((res) => {\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n  }\n}\n"]}]}