4BA6BA,GA7BA,EA6BA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,GAAA,EAAA,CAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,IAAA;AACA,KAhCA;AAkCA,IAAA,cAAA;AAAA,oFAAA,iBAAA,OAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,qBAAA,MAAA,CAAA,MAAA,CAAA,eAAA,EAAA,IAAA;AAEA,gBAAA,MAHA,GAGA;AACA,kBAAA,WAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aADA;AAEA,kBAAA,SAAA,EAAA;AAFA,iBAHA;AAAA;AAAA,uBAQA,KAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,MAAA,CARA;;AAAA;AAQA,gBAAA,GARA;AASA,gBAAA,OAAA,CAAA,GAAA,CAAA,yBAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA;;AACA,oBAAA,GAAA,CAAA,WAAA,IAAA,CAAA,EAAA;AACA,kBAAA,cADA,GACA,GAAA,CAAA,IAAA,CAAA,MAAA,CAAA,CAAA,CADA;AAEA,uBAAA,KAAA,CAAA,QAAA,EAAA,cAAA,EAAA,CAAA;AACA,uBAAA,MAAA,CAAA,MAAA,CAAA,oBAAA,EAAA,IAAA;AAEA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAlCA;AAzBA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div class=\"card\">\n    <Table border :row-class-name=\"rowClassName\" :columns='columns' :data='tradeData.result' :loading=\"loading\">\n      <template slot=\"trade_type\" slot-scope=\"{ row }\">\n        <span class=\"green-dark-color \" v-if=\"row.trade_type==1\">{{ row.trade_type_desc }}</span>\n        <span class=\"red-color \" v-if=\"row.trade_type==2\">{{ row.trade_type_desc }}</span>\n        <span class=\"blue-dark-color \" v-if=\"row.trade_type==3\">{{ row.trade_type_desc }}</span>\n        <span class=\"dark-gray-color \" v-if=\"row.trade_type==4\">{{ row.trade_type_desc }}</span>\n        <span class=\"blue-color \" v-if=\"row.trade_type==5\">{{ row.trade_type_desc }}</span>\n      </template>\n      <template slot=\"batch_no\" slot-scope=\"{ row }\">\n        <span class=\"blue-color pointer_line\" @click=\"handleRefund(row)\" v-if=\"row.is_cancel==0 && row.trade_type==2\">{{ row.batch_no }}</span>\n        <span v-if=\"row.is_cancel==1 || !(row.trade_type==2)\">{{ row.batch_no }}</span>\n      </template>\n      <template slot=\"tradeid\" slot-scope=\"{ row }\">\n        <span class=\"blue-color pointer_line\" @click=\"handleUserRefund(row)\" v-if=\"row.trade_type==2\">{{ row.trade_id }}</span>\n        <span v-if=\"!(row.trade_type==2)\">{{ row.trade_id }}</span>\n      </template>\n      <template slot=\"group\" slot-scope=\"{ row }\">\n        <span>{{ grouptype(row.user_group) }}</span>\n      </template>\n      <template slot=\"iscancel\" slot-scope=\"{ row }\">\n        <span class=\"red-color \" v-if=\"row.is_cancel==1\">{{row.is_cancel?'是':'否'}}</span>\n        <span v-if=\"row.is_cancel==0\"> {{row.is_cancel?'是':'否'}}</span>\n      </template>\n      <template slot=\"relatedid\" slot-scope=\"{ row }\">\n        <span class=\"blue-color pointer_line\" @click=\"getTradeDetail(row.related_id)\" >{{ row.related_id }}</span>\n      </template>\n      <template slot=\"amount\" slot-scope=\"{ row }\">\n        <p class=\"orange-color\"><span v-show=\"row.trade_type==3||row.trade_type==4||row.trade_type==5\">-</span>￥{{row.amount}}</p>\n      </template>\n      <template slot=\"buyer\" slot-scope=\"{ row }\">\n        <span>{{row.usercode?`[${row.usercode}]`:''}}</span>\n        <span> {{row.name?`${row.name}`:''}}</span>\n      </template>\n      <template slot=\"result\" slot-scope=\"{ row }\">\n        <span v-if=\"row.result=='成功'\">{{row.result}}</span>\n        <span class=\"orange-color \" v-if=\"row.result=='失败'\">{{row.result}} : {{row.result_desc}}</span>\n      </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport * as dataConfig from '../dataConfig';\nimport {buildChildDepartmentArray} from \"@/libs/util\";\nexport default {\n  name: 'SearchTable',\n  data() {\n    return {\n      columns: [],\n\n    };\n  },\n\n  components: {},\n\n  computed: {\n    tradeData() {\n      return this.$store.state.trade.tradeData;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n\n    rowClassName (row, index) {\n      if (row.is_cancel == 1) {\n        return 'table-info-row';\n      }\n      return '';\n    },\n\n    grouptype(id) {\n      let val = '';\n      if (id == 1)\n        val = '职员';\n      if (id == 2)\n        val = '处长';\n      if (id == 3)\n        val = '局长';\n      return val;\n    },\n    // 点击批号，打开撤销modal\n    handleRefund(row){\n      this.$emit('params', row, 1);\n      this.$store.commit('setRefundBatchModal', true);\n    },\n\n    // 点击订单号，打开撤销用户modal\n    handleUserRefund(row){\n      this.$emit('params', row, 2);\n      this.$store.commit('setRefundUserModal', true);\n    },\n\n    getTradeDetail: async function (tradeid) {\n      this.$store.commit('TABLE_LOADING', true);\n\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        trade_ids: tradeid,\n      };\n\n      let res = await this.$store.dispatch('tradeDetail', params);\n      console.log(\"getTradeDetail===>>>\" + JSON.stringify(res));\n      if (res.return_code == 0){\n        let curTradeDetail = res.data.result[0];\n        this.$emit('params', curTradeDetail, 2);\n        this.$store.commit('setRefundUserModal', true);\n\n      }\n    },\n\n  }\n}\n</script>\n<style>\n    .ivu-table .table-info-row td{\n      background-color: #B0C4DE;\n    }\n    /deep/ .ivu-table .table-error-row td{\n      background-color: #ff6600;\n    }\n</style>\n<!--<style lang='less' scoped>-->\n<!--    //.ivu-table .demo-table-error-row td{-->\n<!--    //  background-color: #ff6600;-->\n<!--    //  color: #fff;-->\n<!--    //}-->\n<!--    //.ivu-table td.demo-table-info-column{-->\n<!--    //  background-color: #2db7f5;-->\n<!--    //  color: #fff;-->\n<!--    //}-->\n<!--    //.ivu-table .demo-table-info-cell-name {-->\n<!--    //  background-color: #2db7f5;-->\n<!--    //  color: #fff;-->\n<!--    //}-->\n<!--    //.ivu-table .demo-table-info-cell-age {-->\n<!--    //  background-color: #ff6600;-->\n<!--    //  color: #fff;-->\n<!--    //}-->\n<!--    //.ivu-table .demo-table-info-cell-address {-->\n<!--    //  background-color: #187;-->\n<!--    //  color: #fff;-->\n<!--    //}-->\n<!--    /deep/.ivu-table td.order_amount{-->\n<!--      color: #ff602e;-->\n<!--    }-->\n<!--    /deep/.ivu-table-wrapper{-->\n<!--      overflow: visible;-->\n<!--    }-->\n<!--    .refund{-->\n<!--      /deep/.ivu-icon{-->\n<!--        display: none;-->\n<!--        left: 16px;-->\n<!--      }-->\n<!--    }-->\n\n<!--    /deep/.ivu-poptip-title-inner{-->\n<!--      color: #e42329;-->\n<!--      padding: 10px;-->\n<!--    }-->\n<!--</style>-->\n"],"sourceRoot":"src/views/trade/components"}]}