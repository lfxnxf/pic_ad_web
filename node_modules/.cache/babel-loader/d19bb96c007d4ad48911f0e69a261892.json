ACA,iBAAOE,OAAO,CAACG,GAAD,CAAd;AACD,SAHD,WAGS,UAACC,GAAD,EAAS;AAChBN,UAAAA,MAAM,CAAC,eAAD,EAAkB,KAAlB,CAAN;AACAG,UAAAA,MAAM,CAACG,GAAD,CAAN;AACD,SAND;AAOD,OARM,CAAP;AASD;AAhHK;AAxCK,CAAf","sourcesContent":["\nimport * as merchant from '@/api/merchant';\nimport axios from '@/libs/api.request';\nexport default {\n  state:{\n    merchantList: {},\n    merchantListLoading: false,\n    addMerchantModal: false,\n    editMerchantModal: false,\n    currentData: {},\n    merchantData: {},\n    merchantUserList: {},\n    curMerchantUser: {},\n    editMerchantUserModal: false,\n  },\n  getters: {\n  },\n  mutations:{\n    UpdateMerchantList(state, val) {\n      state.merchantList = val;\n    },\n    SetMerchantCurrentData(state, val){\n      state.currentData = val;\n    },\n    setEditMerchantModal(state, val){\n      state.editMerchantModal = val;\n    },\n    setAddMerchantModal(state, val){\n      state.addMerchantModal = val;\n    },\n    UPDATEMERCHANTDATA(state, val) {\n      state.merchantData = val;\n    },\n    UpdateMerchantUserList(state, val){\n      state.merchantUserList = val;\n    },\n    UpdateCurMerchantUser(state, val){\n      state.curMerchantUser = val;\n    },\n    setEditMerchantUserModal(state, val){\n      state.editMerchantUserModal = val;\n    },\n  },\n  actions:{\n    // 获取商户详情\n    merchantDetail({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantDetail(data).then((res) => {\n          commit('UPDATEMERCHANTDATA', res.data)\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n    merchantEdit({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantEdit(data).then((res) => {\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n    // 获取商户列表\n    merchantList({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantList(data).then((res) => {\n          commit('UpdateMerchantList', res.data);\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        })\n          .catch((err) => {\n            commit('TABLE_LOADING', false);\n            reject(err);\n\n          });\n      });\n    },\n    // 添加商户信息\n    merchantAdd({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantAdd(data).then((res) => {\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n\n    icloudSync({ commit }, data) {\n      return new Promise((resolve, reject) => {\n        merchant.icloudSync(data).then((res) => {\n          return resolve(res);\n        })\n          .catch((err) => {\n            reject(err);\n          });\n      });\n    },\n\n    // 商户收银端账户信息列表\n    merchantUserList({commit}, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantUserList(data).then((res) => {\n          commit('UpdateMerchantUserList', res.data)\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        }).catch((err) => {\n          commit('TABLE_LOADING', false);\n          reject(err);\n        });\n      });\n    },\n\n    // 添加商户收银端账户信息\n    merchantUserAdd({commit}, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantUserAdd(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        }).catch((err) => {\n          commit('TABLE_LOADING', false);\n          reject(err);\n        });\n      });\n    },\n\n    // 编辑商户收银端账户信息\n    merchantUserEdit({commit}, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantUserEdit(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        }).catch((err) => {\n          commit('TABLE_LOADING', false);\n          reject(err);\n        });\n      });\n    },\n\n    // 添加商户收银端账户信息\n    merchantUserDetail({commit}, data) {\n      return new Promise((resolve, reject) => {\n        merchant.merchantUserDetail(data).then((res) => {\n          commit('TABLE_LOADING', false);\n          return resolve(res);\n        }).catch((err) => {\n          commit('TABLE_LOADING', false);\n          reject(err);\n        });\n      });\n    },\n  }\n}\n"]}]}