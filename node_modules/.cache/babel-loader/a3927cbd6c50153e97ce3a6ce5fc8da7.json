AAA,KAAA,IAAA,GAAA,EAAA,GAAA,KAAA,IAAA,CAAA,WAAA,CAAA,QAAA,EAJA;AAKA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QAAA,CAAA,CAAA,KAAA,IAAA,GAAA,EAAA,GAAA,KAAA,IAAA,CAAA,QAAA,CAAA,QAAA,EALA;AAMA,QAAA,UAAA,EAAA,KAAA,IAAA,CAAA,KAAA,CAAA,CAAA,KAAA,IAAA,GAAA,EAAA,GAAA,KAAA,IAAA,CAAA,KAAA,CAAA,QAAA,EANA;AAOA,QAAA,GAAA,EAAA,KAAA,IAAA,CAAA,GAPA;AAQA,QAAA,WAAA,EAAA,KAAA,IAAA,CAAA,WARA;AASA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QATA;AAUA,QAAA,MAAA,EAAA,KAAA,IAAA,CAAA,MAAA,CAAA,CAAA,KAAA,IAAA,GAAA,EAAA,GAAA,KAAA,IAAA,CAAA,MAAA,CAAA,QAAA,EAVA;AAWA,QAAA,SAAA,EAAA,KAAA,IAAA,CAAA,SAAA,IAAA,IAAA,GAAA,EAAA,GAAA,KAAA,IAAA,CAAA,SAAA,CAAA,QAAA;AAXA,OAAA;AAaA,aAAA,IAAA;AACA,KAhBA;AAkBA,IAAA,QAlBA,sBAkBA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA;AACA;AApBA,GAtBA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,wBACA,GADA,EACA,IADA,EACA;AACA,UAAA,CAAA,CAAA,OAAA,CAAA,GAAA,EAAA,IAAA,KAAA,CAAA,CAAA,EAAA;AACA,aAAA,IAAA,CAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA;AACA,KALA;AAMA;AACA,IAAA,SAAA;AAAA,+EAAA,iBAAA,IAAA,EAAA,SAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA;AACA,kBAAA,WAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aADA;AAEA,kBAAA,SAAA,EAAA,CAFA;AAGA,kBAAA,IAAA,EAAA,IAAA,IAAA,CAHA;AAIA,kBAAA,SAAA,EAAA,SAAA,IAAA,KAAA,OAAA,CAAA;AAJA,iBADA;AAAA;AAAA,uBAOA,KAAA,MAAA,CAAA,QAAA,CAAA,UAAA,EAAA,MAAA,CAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,OAPA;AAgBA,IAAA,cAhBA,0BAgBA,GAhBA,EAgBA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,GAAA;AACA;AAlBA,GA9CA;AAmEA,EAAA,OAnEA,qBAmEA;AACA,SAAA,SAAA;AACA;AArEA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div class=\"card\">\n    <Form inline ref=\"form\" :model=\"form\" label-colon>\n\n      <FormItem label=\"交易日期起止\">\n        <DatePicker @keyup.enter.native=\"$parent.query()\" v-model=\"form.date\" type=\"daterange\" format=\"yyyy/MM/dd\" placeholder=\"就餐日期起止\" style=\"width: 200px\"></DatePicker>\n      </FormItem>\n      <FormItem>\n        <Input :style=\"{width: '230px'}\" clearable @keyup.enter.native=\"$parent.query()\" v-model=\"form.key\" placeholder=\"姓名/手机号/身份证号/就餐卡号\" />\n      </FormItem>\n      <FormItem>\n        <Input :style=\"{width: '120px'}\" clearable @keyup.enter.native=\"$parent.query()\" v-model=\"form.batch_no\" placeholder=\"批次号\" />\n      </FormItem>\n      <FormItem>\n        <Input :style=\"{width: '100px'}\" clearable @keyup.enter.native=\"$parent.query()\" v-model=\"form.related_ids\" placeholder=\"关联交易ID\" />\n      </FormItem>\n      <FormItem>\n        <Select placeholder=\"交易类型\" @keyup.enter.native=\"$parent.query()\" v-model=\"form.trade_types\" multiple @on-change=\"handleSelect(form.trade_types,'trade_types')\"  style=\"min-width: 100px;\">\n          <Option value=\"-1\">全部交易类型</Option>\n          <Option value=\"1\" :disabled=\"form.trade_types=='-1'\">消费</Option>\n          <Option value=\"2\" :disabled=\"form.trade_types=='-1'\">充值</Option>\n          <Option value=\"3\" :disabled=\"form.trade_types=='-1'\">提现</Option>\n          <Option value=\"4\" :disabled=\"form.trade_types=='-1'\">退款</Option>\n          <Option value=\"5\" :disabled=\"form.trade_types=='-1'\">充值撤销</Option>\n        </Select>\n      </FormItem>\n      <FormItem>\n       <Select placeholder=\"交易结果\" multiple @keyup.enter.native=\"$parent.query()\" v-model=\"form.status\" @on-change=\"handleSelect(form.status,'status')\"  style=\"min-width: 100px;\">\n         <Option value=\"-1\">全部交易结果</Option>\n         <Option value=\"success\">交易成功</Option>\n         <Option value=\"failed\">交易失败</Option>\n         <Option value=\"trading\">交易中</Option>\n       </Select>\n     </FormItem>\n     \n      <FormItem>\n        <Select placeholder=\"人员类型\" @keyup.enter.native=\"$parent.query()\" v-model=\"form.role_ids\" multiple @on-change=\"handleSelect(form.role_ids,'role_ids')\" style=\"min-width:100px;\">\n          <Option value=\"-1\" >全部人员类型</Option>\n          <Option v-for=\"item in roleList\" :disabled=\"form.role_ids=='-1'\" :key=\"item.role_id\" :value=\"item.role_id\" :label=\"item.name\">\n            {{ item.name }}\n          </Option>\n        </Select>\n      </FormItem>\n      <FormItem>\n        <Select placeholder=\"是否撤销\" @keyup.enter.native=\"$parent.query()\" v-model=\"form.is_cancel\" style=\"min-width: 110px;\">\n          <Option value=\"-1\">是否撤销-全部</Option>\n          <Option value=\"0\" >是否撤销-否</Option>\n          <Option value=\"1\" >是否撤销-是</Option>\n        </Select>\n      </FormItem>\n<!--      <FormItem>-->\n<!--        <Select placeholder=\"职务\" @keyup.enter.native=\"$parent.query()\" v-model=\"form.group\" multiple @on-change=\"handleSelect(form.group,'group')\" style=\"min-width: 100px;\">-->\n<!--          <Option value=\"-1\" >全部职务</Option>-->\n<!--          <Option :disabled=\"form.status_str=='-1'\" value=\"1\">职员</Option>-->\n<!--          <Option :disabled=\"form.status_str=='-1'\" value=\"2\">处长</Option>-->\n<!--          <Option :disabled=\"form.status_str=='-1'\" value=\"3\">局长</Option>-->\n<!--        </Select>-->\n<!--      </FormItem>-->\n      <FormItem class=\"departments_tree\">\n        <department-tree @formDepartments=\"getDepartments\"></department-tree>\n      </FormItem>\n     \n      <FormItem>\n        <Button type='primary' icon=\"md-search\" @click=\"$parent.query()\">查询</Button>\n        <Button class=\"margin-left-10\" type='primary' ghost icon=\"md-download\" @click=\"$parent.exportTradeRecord()\">导出EXCEL</Button>\n      </FormItem>\n      <!-- <FormItem>\n        <Button type='info' icon=\"md-download\">导出</Button>\n      </FormItem> -->\n\n    </Form>\n  </div>\n</template>\n<script>\nimport _ from 'underscore';\nimport moment from \"moment-timezone\";\nimport departmentTree from '@/components/department-tree';\nimport {buildChildDepartmentArray} from \"@/libs/util\";\nexport default {\n  name: 'searchForm',\n  data() {\n    return {\n      form: {\n        date: [moment().format('YYYY-MM-DD'), moment().format('YYYY-MM-DD')],\n        trade_types: [],\n        departments: [],\n        role_ids: [],  \n        group: '',\n        key: '',\n        related_ids: '',\n        batch_no: '',\n        is_cancel: '',\n        status: []\n      },\n\n    };\n  },\n\n  components: { departmentTree },\n\n  computed: {\n    resultForm(){\n      let data = {\n        start_date: moment(this.form.date[0]).format('YYYY-MM-DD'),\n        end_date: moment(this.form.date[1]).format('YYYY-MM-DD'),\n        trade_types: this.form.trade_types[0]=='-1'? '':this.form.trade_types.toString(),\n        departments: this.form.departments[0]=='-1'? '':this.form.departments.toString(),\n        role_ids: this.form.role_ids[0]=='-1'? '':this.form.role_ids.toString(),\n        user_group: this.form.group[0]=='-1'? '':this.form.group.toString(),\n        key: this.form.key,\n        related_ids: this.form.related_ids,\n        batch_no: this.form.batch_no,\n        status: this.form.status[0]=='-1'?'':this.form.status.toString(),\n        is_cancel: this.form.is_cancel=='-1'?'':this.form.is_cancel.toString(),\n      }\n      return data;\n    },\n\n    roleList() {\n      return this.$store.state.role.roleList.result;\n    },\n\n  },\n\n  methods: {\n    handleSelect(val, name){\n      if(_.indexOf(val, '-1') != -1){\n        this.form[name] = ['-1'];\n      }\n    },\n    //获取人员类型列表\n    queryRole: async function(page, page_size) {\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        is_enable: 1,\n        page: page || 1,\n        page_size: page_size || this.$parent.page_size\n      };\n      await this.$store.dispatch('roleList', params);\n    },\n    getDepartments(val){\n      this.form.departments = val;\n    },\n\n  },\n  mounted() {\n    this.queryRole();\n  }\n}\n</script>\n<style lang='less' scoped>\n.card{\n  padding-bottom: 0 !important;\n  // min-height: 76px;\n  // max-height: 133px;\n}\n</style>\n"],"sourceRoot":"src/views/trade/components"}]}