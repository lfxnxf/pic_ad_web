rEA,0BAqEA,GArEA,EAqEA;AACA,WAAA,IAAA,CAAA,WAAA,GAAA,GAAA;AACA,KAvEA;AAyEA;AACA,IAAA,QA1EA,oBA0EA,OA1EA,EA0EA;AACA,UAAA,CAAA,OAAA,EAAA;AACA;AACA;;AAEA,UAAA,IAAA,GAAA,QAAA,CAAA,aAAA,CAAA,GAAA,CAAA;AACA,MAAA,IAAA,CAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,MAAA,IAAA,CAAA,IAAA,GAAA,OAAA;AACA,MAAA,IAAA,CAAA,YAAA,CAAA,UAAA,EAAA,OAAA;AACA,MAAA,QAAA,CAAA,IAAA,CAAA,WAAA,CAAA,IAAA;AACA,MAAA,IAAA,CAAA,KAAA,GAVA,CAWA;AACA;AAtFA,GAhDA;AAwIA,EAAA,OAxIA,qBAwIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,KAAA,CAAA,UAAA;;AACA,cAAA,KAAA,CAAA,aAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA;AA3IA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div class=\"order-list card\">\n    <Form inline ref=\"form\" :model=\"form\" label-colon>\n      <FormItem label=\"关键字\">\n        <Input :style=\"{width: '200px'}\" clearable v-model=\"form.key\" placeholder=\"姓名/手机号/卡号/证件号\" />\n      </FormItem>\n       <FormItem label=\"就餐日期\">\n        <DatePicker v-model=\"form.dinner_time\" @keyup.enter.native=\"listForPortal()\" type=\"datetimerange\" format=\"yyyy/MM/dd\" placeholder=\"就餐日期起止\" style=\"width: 200px\"></DatePicker>\n      </FormItem>\n     \n      <FormItem label=\"预约日期\">\n        <DatePicker v-model=\"form.appointment_time\" type=\"datetimerange\" format=\"yyyy/MM/dd\" placeholder=\"预约日期起止\" style=\"width: 200px\"></DatePicker>\n      </FormItem>\n      <FormItem label=\"预约就餐日期\">\n        <DatePicker v-model=\"form.date\" type=\"daterange\" format=\"yyyy/MM/dd\" placeholder=\"预约就餐日期起止\" style=\"width: 200px\"></DatePicker>\n      </FormItem>\n    \n     \n      <FormItem label=\"餐种\">\n        <Select \n          placeholder=\"请选择餐种\" \n          multiple \n          @keyup.enter.native=\"listForPortal()\" \n          v-model=\"form.goods_ids\" \n          @on-change=\"handleSelect(form.goods_ids,'goods_ids')\" \n          style=\"min-width: 100px;\">\n          <Option value=\"-1\" >全部餐种</Option>\n          <Option\n            v-for=\"item in goodsList\"\n            :key=\"item.goods_id\"\n            :disabled=\"form.goods_ids=='-1'\"\n            :value=\"item.goods_id+''\">{{item.goods_name}}\n          </Option>\n        </Select>\n      </FormItem>\n       <FormItem label=\"人员类型\">\n        <Select \n          placeholder=\"请选择人员类型\" \n          multiple \n          @keyup.enter.native=\"listForPortal()\" \n          v-model=\"form.role_ids\" \n          @on-change=\"handleSelect(form.role_ids,'role_ids')\" \n          style=\"min-width: 100px;\">\n          <Option value=\"-1\" >全部人员类型</Option>\n          <Option\n            v-for=\"item in roleList\"\n            :key=\"item.role_id\"\n            :disabled=\"form.role_ids=='-1'\"\n            :label=\"item.name\"\n            :value=\"item.role_id\">{{item.name}}\n          </Option>\n        </Select>\n      </FormItem>\n      <FormItem class=\"departments_tree\" label=\"部门\">\n        <department-tree @formDepartments=\"getDepartments\"></department-tree>\n      </FormItem>\n      <FormItem label=\"订单状态\">\n        <Select \n          placeholder=\"订单状态\" \n          @keyup.enter.native=\"listForPortal()\" \n          v-model=\"form.status_str\" \n          multiple \n          @on-change=\"handleSelect(form.status_str,'status_str')\" \n          style=\"min-width: 100px;\">\n          <Option value=\"-1\" >全部订单状态</Option>\n          <Option :disabled=\"form.status_str=='-1'\" value=\"10\">待付款</Option>\n          <Option :disabled=\"form.status_str=='-1'\" value=\"50\">已完成</Option>\n          <Option :disabled=\"form.status_str=='-1'\" value=\"70\">已退款</Option>\n          <Option :disabled=\"form.status_str=='-1'\" value=\"0\">已取消</Option>\n        </Select>\n      </FormItem>\n     \n      <FormItem >\n        <Button type='primary' icon=\"md-search\" @click=\"listForPortal()\">查询</Button>\n        <Button class=\"margin-left-10\" type='primary' ghost icon=\"md-download\" @click=\"exportDiningRecord\">导出EXCEL</Button>\n      </FormItem>\n\n    </Form>\n  </div>\n</template>\n<script>\nimport _ from 'underscore';\nimport moment from \"moment-timezone\";\nimport {buildChildDepartmentArray} from \"@/libs/util\";\nimport departmentTree from '@/components/department-tree';\n\nexport default {\n  name: 'searchForm',\n  data() {\n    return {\n      form: {\n        key: '',\n        date: [],\n        appointment_time: [],\n        dinner_time: [moment().format('YYYY-MM-DD'), moment().format('YYYY-MM-DD')],\n        goods_ids: [],\n        role_ids: [],\n        status_str: [],\n        departments: []\n      },\n\n    };\n  },\n\n  components: { departmentTree },\n\n  computed: {\n    resultForm(){\n      let data = {\n        status_str: this.form.status_str[0]=='-1'? '':this.form.status_str.toString(),\n        start_date: this.form.date[0]==''? '':moment(this.form.date[0]).format('YYYY-MM-DD'),\n        end_date: this.form.date[1]==''? '':moment(this.form.date[1]).format('YYYY-MM-DD'),\n        start_appointment_time: this.form.appointment_time[0]==''? '':moment(this.form.appointment_time[0]).format('YYYY-MM-DD'),\n        end_appointment_time: this.form.appointment_time[1]==''? '':moment(this.form.appointment_time[1]).format('YYYY-MM-DD'),\n        start_dinner_time: this.form.dinner_time[0]==''? '':moment(this.form.dinner_time[0]).format('YYYY-MM-DD'),\n        end_dinner_time: this.form.dinner_time[1]==''? '':moment(this.form.dinner_time[1]).format('YYYY-MM-DD'),\n        goods_ids: this.form.goods_ids[0]=='-1'? '':this.form.goods_ids.toString(),\n        role_ids: this.form.role_ids[0]=='-1'? '':this.form.role_ids.toString(),\n        departments: this.form.departments[0]=='-1'? '':this.form.departments.toString(),\n        key: this.form.key\n      }\n      return data;\n    },\n    goodsList() {\n      return this.$store.state.goods.goodsList.result;\n    },\n    roleList() {\n      return this.$store.state.role.roleList.result;\n    },\n    currentMerchantId(){\n      return this.$store.state.user.curMerchantId;\n    },\n  },\n\n  methods: {\n    listForPortal: function(page) {\n      this.$store.commit('TABLE_LOADING', true);\n      let startDate = this.resultForm.start_dinner_time || '';\n      let endDate = this.resultForm.end_dinner_time || '';\n      if (startDate == '' || startDate == 'Invalid date' || endDate == '' || endDate == 'Invalid date'){\n        this.$Message.error(\"请选择就餐日期起止！\");\n        return;\n      }\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        page: page || 1,\n        page_size: this.$parent.page_size,\n        ...this.resultForm\n      };\n      console.log(params)\n      this.$store.dispatch('orderList', params);\n    },\n    // 获取餐种list\n    queryGoods: async function() {\n      let params = {\n        merchant_id: this.currentMerchantId,\n        page: 1,\n        page_size: 100,\n        is_enable: 1\n      };\n      await this.$store.dispatch('goodsList', params);\n\n    },\n    //查询角色\n    queryRoleList: async function() {\n      let params = {\n        merchant_id: this.currentMerchantId,\n        is_enable: 1,\n        page: 1,\n        page_size: 10000\n      };\n      await this.$store.dispatch('roleList', params);\n\n    },\n    //多选select\n    handleSelect(val, name){\n      if(_.indexOf(val, '-1') != -1){\n        this.form[name] = ['-1'];\n      }\n    },\n\n    exportDiningRecord: async function() {\n      let fileName = '就餐列表';\n      this.$store.commit('TABLE_LOADING', true);\n\n      let startDate = this.resultForm.start_date || '';\n      let endDate = this.resultForm.end_date || '';\n      if (startDate == '' || startDate == 'Invalid date' || endDate == '' || endDate == 'Invalid date'){\n        this.$Message.error(\"请选择就餐日期起止！\");\n        return;\n      }\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        ...this.resultForm\n      };\n\n      let res = await this.$store.dispatch('exportDiningList', params);\n      if (res.return_code == 0){\n        let fileUrl = res.data.url;\n        this.download(fileUrl);\n      }\n      this.$store.commit('TABLE_LOADING', false);\n    },\n    getDepartments(val){\n      this.form.departments = val;\n    },\n\n    // 下载文件\n    download(fileUrl) {\n      if (!fileUrl) {\n        return;\n      }\n\n      let link = document.createElement(\"a\");\n      link.style.display = \"none\";\n      link.href = fileUrl;\n      link.setAttribute(\"download\", fileUrl);\n      document.body.appendChild(link);\n      link.click();\n      // this.$Message.success(\"下载完成！\");\n    },\n  },\n  async mounted() {\n    this.queryGoods();\n    this.queryRoleList();\n  }\n}\n</script>\n<style lang='less' scoped>\n.card{\n  padding-bottom: 0 !important;\n\n}\n</style>\n"],"sourceRoot":"src/views/dining/components"}]}