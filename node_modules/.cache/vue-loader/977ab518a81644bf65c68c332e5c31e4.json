   </template>\n      <template slot=\"order_amount\" slot-scope=\"{ row }\">\n        <span>￥{{row.order_amount}}</span>\n      </template>\n      <template slot=\"buyer\" slot-scope=\"{ row }\">\n        <span>{{row.buyer_usercode?`[${row.buyer_usercode}]`:''}}</span>\n        <span> {{row.buyer_name?`${row.buyer_name}`:''}}</span>\n      </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport * as dataConfig from '../dataConfig';\nexport default {\n  name: 'SearchTable',\n  data() {\n    return {\n      columns: [],\n    };\n  },\n\n  components: {},\n\n  computed: {\n    reserveList() {\n      return this.$store.state.appointment.reserveList;\n    },\n    loading() {\n      return this.$store.state.appointment.reserveListLoading;\n    },\n    status_color(){\n      return val => {\n        let color = '';\n        switch(val){\n          case 0:\n            color = \"default\";\n            break;\n          case 1:\n            color = \"volcano\";\n            break;\n          case 2:\n            color = \"green\";\n            break;\n          case 3:\n            color = \"blue\";\n        }\n        return color;\n      };\n    }\n    \n  },\n\n  mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    }\n  },\n  filters: {\n    statusColor(val) {\n      let color = '';\n      switch(val){\n        case '0':\n          color = \"default\";\n          break;\n        case '1':\n          color = \"volcano\";\n          break;\n        case '2':\n          color = \"green\";\n          break;\n        case '3':\n          color = \"blue\";\n      }\n      return color;\n    }\n  }\n}\n</script>\n<style lang='less' scoped>\n    /deep/.ivu-table td.order_amount{\n        color: #ff602e;\n    }\n</style>"]}]}