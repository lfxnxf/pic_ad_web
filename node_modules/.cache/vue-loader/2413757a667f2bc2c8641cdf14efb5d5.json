AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"test.vue","sourceRoot":"src/views/platform","sourcesContent":["<template>\n  <Row :gutter=\"32\">\n    <Col span=\"12\">\n      <Card title=\"测试用户登录\">\n        <Form ref=\"formInline\" :model=\"formInline\" :rules=\"ruleInline\" inline>\n          <FormItem prop=\"username\">\n            <Input type=\"text\" v-model=\"formInline.username\" placeholder=\"Username\">\n              <Icon type=\"ios-person-outline\" slot=\"prepend\"></Icon>\n            </Input>\n          </FormItem>\n          <FormItem prop=\"password\">\n            <Input\n              type=\"password\"\n              v-model=\"formInline.password\"\n              placeholder=\"Password\"\n            >\n              <Icon type=\"ios-lock-outline\" slot=\"prepend\"></Icon>\n            </Input>\n          </FormItem>\n          <FormItem>\n            <Button type=\"primary\" @click=\"handleSubmit('formInline')\"\n              >Signin</Button\n            >\n          </FormItem>\n        </Form>\n        <!-- <Divider v-if=\"sessionid\" /> -->\n        <Card v-if=\"sessionid\" :bordered=\"false\">\n          <div style=\"text-align:center\">\n            <Icon type=\"md-checkmark-circle\" color=\"#19be6b\" :size=\"64\"/>\n            <h3>登录成功！</h3>\n        </div>\n        </Card>\n      </Card>\n    </Col>\n    <Col span=\"12\">\n      <Card title=\"测试用户信息\">\n        <Button\n          slot=\"extra\"\n          size=\"small\"\n          type=\"success\"\n          :disabled=\"!sessionid\"\n          @click=\"handleGetUserInfo(sessionid)\"\n          >获取用户信息</Button\n        >\n        <p v-if=\"UCUser && Object.keys(UCUser).length > 0\">{{JSON.stringify(UCUser)}}</p>\n      </Card>\n    </Col>\n    <Col span=\"12\">\n      <Card title=\"测试门禁接口\">\n        <Form ref=\"formInCard\" :model=\"formInCard\" >\n          <FormItem prop=\"empid\">\n            <Input type=\"text\" v-model=\"formInCard.emp_id\" placeholder=\"企业员工ID\">\n              <Icon type=\"ios-person-outline\" slot=\"prepend\"></Icon>\n            </Input>\n          </FormItem>\n          <FormItem prop=\"cardno\">\n            <Input type=\"text\" v-model=\"formInCard.card_no\" placeholder=\"就餐卡号\">\n              <Icon type=\"ios-person-outline\" slot=\"prepend\"></Icon>\n            </Input>\n          </FormItem>\n          <FormItem prop=\"deviceno\">\n            <Input type=\"text\" v-model=\"formInCard.device_code\" placeholder=\"设备号\">\n              <Icon type=\"ios-person-outline\" slot=\"prepend\"></Icon>\n            </Input>\n          </FormItem>\n          <FormItem>\n            <Button type=\"primary\" @click=\"handleSubmitCard('formInCard')\"\n            >就餐验证</Button\n            >\n          </FormItem>\n        </Form>\n\n      </Card>\n    </Col>\n    <Col span=\"12\">\n      <Card title=\"测试树状组件\">\n        <Button\n          slot=\"extra\"\n          size=\"small\"\n          type=\"success\"\n          @click=\"handleTreeInfo\">测试树状组件</Button>\n        <tree :tree='treeData'></tree>\n      </Card>\n    </Col>\n    <Col span=\"12\">\n      <Card title=\"测试树组件返回节点字符串\">\n        <Button\n          slot=\"extra\"\n          size=\"small\"\n          type=\"success\"\n          @click=\"handleTreeNodeInfo\">测试树组件返回节点字符串</Button>\n      </Card>\n    </Col>\n  </Row>\n\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport crypto from 'crypto'\nimport tree from './tree'\nimport {buildTree} from \"@/libs/util\";\nimport {ExportSavePdf, previewImg, getTreeNodes, getTreeNodeItem, getTreeNodesValue} from \"@/libs/util\";\n\nexport default {\n  name: \"PlatformTest\",\n  components: { tree },\n  data() {\n    return {\n      treeDataItem: [],\n      formInline: {\n        username: \"\",\n        password: \"\",\n      },\n      ruleInline: {\n        username: [\n          {\n            required: true,\n            message: \"Please fill in the user name\",\n            trigger: \"blur\",\n          },\n        ],\n        password: [\n          {\n            required: true,\n            message: \"Please fill in the password.\",\n            trigger: \"blur\",\n          },\n          {\n            type: \"string\",\n            min: 6,\n            message: \"The password length cannot be less than 6 bits\",\n            trigger: \"blur\",\n          },\n        ],\n      },\n      formInCard: {\n        emp_id: \"e7M8FcLqP5LZYDLCN\",\n        device_code: \"P541722WB1570172\",\n        card_no: \"\",\n      },\n      treeData: [{\n        \"id\": 1,\n        \"name\": \"必体验\",\n        \"child\": []\n      }, {\n        \"id\": 2,\n        \"name\": \"休闲生活\",\n        \"child\": [{\n          \"id\": 15,\n          \"level\": 2,\n          \"name\": \"热门城市\",\n          \"child\": [{\n            \"id\": 5,\n            \"name\": \"乌鲁木齐\",\n            \"child\": []\n          }, {\n            \"id\": 6,\n            \"name\": \"北京\",\n            \"child\": []\n          }, {\n            \"id\": 7,\n            \"name\": \"南京\",\n            \"child\": []\n          }, {\n            \"id\": 8,\n            \"name\": \"昌吉\",\n            \"child\": []\n          }, {\n            \"id\": 9,\n            \"name\": \"西安\",\n            \"child\": []\n          }]\n        }, {\n          \"id\": 16,\n          \"name\": \"主题\",\n          \"child\": [{\n            \"id\": 10,\n            \"name\": \"城市经典\",\n            \"child\": []\n          }, {\n            \"id\": 11,\n            \"name\": \"四季美食\",\n            \"child\": []\n          }, {\n            \"id\": 12,\n            \"name\": \"周末减压\",\n            \"child\": []\n          }, {\n            \"id\": 13,\n            \"name\": \"亲子成长\",\n            \"child\": []\n          }, {\n            \"id\": 14,\n            \"name\": \"聚会团建\",\n            \"child\": []\n          }]\n        }]\n      }, {\n        \"id\": 3,\n        \"name\": \"慢旅\",\n        \"child\": []\n      }, {\n        \"id\": 4,\n        \"name\": \"酒店民宿\",\n        \"child\": []\n      }],\n      treeData2: [\n        {\n          \"code\": \"7001\",\n          \"name\": \"项目支出\",\n          \"parents\": \"0\"\n        },\n        {\n          \"code\": \"7101\",\n          \"name\": \"人力资源支出\",\n          \"parents\": \"0\",\n          \"children\": [\n            {\n              \"code\": \"710102\",\n              \"name\": \"职工福利费\",\n              \"parents\": \"7101\"\n            },\n            {\n              \"code\": \"710103\",\n              \"name\": \"工会经费\",\n              \"parents\": \"7101\",\n              \"children\": [\n                {\n                  \"code\": \"71010301\",\n                  \"name\": \"gonghui职工福利费\",\n                  \"parents\": \"710103\"\n                },\n                {\n                  \"code\": \"71010302\",\n                  \"name\": \"gonghui工会经费\",\n                  \"parents\": \"710103\"\n                }\n              ]\n            }\n          ]\n        },\n        {\n          \"code\": \"7103\",\n          \"name\": \"市场推广支出\",\n          \"parents\": \"0\"\n        }\n      ],\n    };\n  },\n  computed: {\n    ...mapState({\n      sessionid: (state) => state.platform.sessionid,\n      UCUser: (state) => state.platform.UCUser,\n    }),\n    departmentList() {\n      return this.$store.state.user.empDepartmentList;\n    },\n  },\n  methods: {\n    ...mapActions([\"platformUserLogin\", \"platformUserInfo\", \"platformCardLogin\"]),\n    handleSubmit(name) {\n      this.$refs[name].validate((valid) => {\n        if (valid) {\n          const password = crypto.createHash('sha1').update(this.$refs[name].model.password, 'utf8').digest('hex').toLowerCase();\n          this.platformUserLogin({\n            data: {\n              ...this.$refs[name].model,\n              password: password,\n              method: \"ih.user.auth.login\",\n            },\n            params: { method: \"ih.user.auth.login\" },\n            method: 'post'\n          }).then(res => {\n            if (res.return_code === 0) {\n              this.$refs[name].resetFields();\n              this.$Message.success(\"Success!\");\n            }\n          });\n        } else {\n          this.$Message.error(\"Fail!\");\n        }\n      });\n    },\n    handleGetUserInfo(sessionid) {\n      this.platformUserInfo({\n        data: { sessionid: sessionid, method: \"ih.user.info.getUserInfoDetail\" },\n        params: { method: \"ih.user.info.getUserInfoDetail\" },\n        method: 'post'\n      });\n    },\n    handleSubmitCard(name) {\n      this.$refs[name].validate((valid) => {\n        if (valid) {\n          this.platformCardLogin({\n            data: {\n              ...this.$refs[name].model,\n              method: \"ih.p_canteen.order.submit\",\n            },\n            params: { method: \"ih.p_canteen.order.submit\" },\n            method: 'post'\n          }).then(res => {\n            if (res.return_code === 0) {\n              this.$refs[name].resetFields();\n              this.$Message.success(\"Success!\");\n            }\n          });\n        } else {\n          this.$Message.error(\"Fail!\");\n        }\n      });\n    },\n    handleTreeInfo() {\n      console.log(\"testvue====treeData===>>>>>\" + JSON.stringify(this.treeData));\n      if (this.departmentList.length > 0) {\n        let departArray = buildTree(this.departmentList, {}, 'department_id', 'parentid', 'child');\n        console.log(\"testvue===buildtree====>>>>>\" + JSON.stringify(departArray));\n        this.treeData = departArray;\n      }\n    },\n    handleTreeNodeInfo() {\n      console.log(\"handleTreeNodeInfo====treeData===>>>>>\" + JSON.stringify(this.treeData2));\n      // if (this.treeData2.length > 0) {\n      //   let departArray = buildTree(this.treeData2, {}, 'department_id', 'parentid', 'children');\n      //   console.log(\"handleTreeNodeInfo===buildtree====>>>>>\" + JSON.stringify(departArray));\n      //   this.treeData2 = departArray;\n      // }\n\n      let itemData = getTreeNodeItem(this.treeData2,'name', 'children', 'name');\n      console.log(\"handleTreeNodeInfo---find itemData equals>>>\" + JSON.stringify(itemData));\n\n      let departArray = getTreeNodesValue(this.treeData2, 'name', 'children');\n      console.log(\"handleTreeNodeInfo---departArray>>>\" + departArray);\n\n      let arrayResult = departArray.join(',');\n      console.log(\"handleTreeNodeInfo---arrayResult>>>\" + arrayResult);\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped></style>\n"]}]}