 _ from 'underscore';\nimport {localRead} from \"@/libs/util\";\n\nexport default {\n  name: 'searchForm',\n  data() {\n    return {\n      loading: false,\n      form: {\n        org_id: '',\n        key: '',\n      },\n    };\n  },\n\n  components: {},\n\n  computed: {\n    getOrgId(){\n      return this.$store.state.user.curOrgId;\n    }\n  },\n\n  methods: {\n    listForPortal: function(page, page_size) {\n      this.$store.commit('TABLE_LOADING', true);\n      if (this.getOrgId != null && this.getOrgId.length > 0) {\n        let params = {\n          key: this.form.key,\n          org_id: this.getOrgId,\n          page: page || 1,\n          page_size: this.$parent.page_size,\n        };\n        this.$store.dispatch('merchantList', params);\n      }\n    },\n    handleAdd(){\n      this.$store.commit('setAddMerchantModal', true);\n    }\n\n\n  },\n  mounted() {\n    this.listForPortal();\n  }\n}\n</script>\n<style lang='less' scoped>\n.card{\n  padding-bottom: 0 !important;\n}\n</style>\n"]}]}