e: ''\n    };\n  },\n\n  components: {\n    searchForm, searchTable,\n  },\n\n  computed: {\n    statisticsList() {\n      return this.$store.state.statistics.statAppointment;\n    },\n    currentMerchantId(){\n      return this.$store.state.user.curMerchantId;\n    },\n  },\n\n  methods: {\n    //查询角色\n    queryRoleList: async function() {\n      let params = {\n        merchant_id: this.currentMerchantId,\n        is_enable: 1,\n        page: 1,\n        page_size: 10000\n      };\n      await this.$store.dispatch('roleList', params);\n\n    },\n\n    // 获取餐种list\n    queryGoods: async function() {\n      let params = {\n        merchant_id: this.currentMerchantId,\n        page: 1,\n        page_size: 100,\n        is_enable: 1\n      };\n      await this.$store.dispatch('goodsList', params);\n\n    },\n    handlePageDown(curPage){\n      this.$refs.FormQuery.queryStat(curPage);\n    },\n    getDate(val){\n      this.start_date = val.start;\n      this.end_date = val.end;\n    },\n\n  },\n\n  mounted(){\n    this.queryRoleList();\n    this.queryGoods();\n\n  },\n  activated(){\n    this.$refs.FormQuery.queryStat();\n  }\n}\n\n</script>\n<style lang='less' scoped>\n</style>\n"]}]}