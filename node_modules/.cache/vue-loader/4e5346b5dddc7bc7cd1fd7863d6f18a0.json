\n  },\n\n  methods: {\n    \n\n    //翻页\n    handlePageDown: function(currentPage) {\n      this.$refs.FormQuery.getMenuFoodData(currentPage, this.page_size);\n    }\n  },\n  \n  activated(){\n    this.$refs.FormQuery.getMenuFoodData(1, this.page_size);\n  }\n\n}\n\n</script>\n<style lang='less' scoped>\n\n</style>"]}]}