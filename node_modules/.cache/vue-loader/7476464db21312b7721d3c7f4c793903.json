 :columns='columns' :data='tableData' :loading=\"loading\">\n        <template slot=\"trade_type\" slot-scope=\"{ row }\">\n          <span>{{ row.trade_type_desc }}</span>\n        </template>\n        <template slot=\"amount\" slot-scope=\"{ row }\">\n          <p class=\"orange-color\"><span v-show=\"row.trade_type==3||row.trade_type==4\">-</span>￥{{row.amount}}</p>\n        </template>\n        <template slot=\"buyer\" slot-scope=\"{ row }\">\n          <span>{{row.usercode?`[${row.usercode}]`:''}}</span>\n          <span> {{row.name?`${row.name}`:''}}</span>\n        </template>\n\n      </Table>\n      <Page class=\"page\" ref=\"pages\" :total=\"total\" :page-size=\"page_size\" size=\"small\" show-total @on-change=\"handlePageDown\"></Page>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as dataConfig from '../dataConfig';\nimport _ from 'underscore';\nimport moment from \"moment-timezone\";\nimport {buildChildDepartmentArray} from \"@/libs/util\";\nexport default {\n  name: 'PrePaid',\n  props: {\n    routeParams: Object\n  },\n  data () {\n    return {\n      date: [moment().format('YYYY-MM-DD'), moment().format('YYYY-MM-DD')],\n      balance: 0,\n      columns: [],\n      tableData: [],\n      formSearch: {},\n      page_size: 50,\n      total: 0,\n    };\n  },\n\n  components: {},\n\n  computed: {\n    resultForm(){\n      let data = {\n        start_date: moment(this.date[0]).format('YYYY-MM-DD'),\n        end_date: moment(this.date[1]).format('YYYY-MM-DD'),\n        trade_types: '',\n        departments: '',\n        role_ids: '',\n        users: this.$route.params.usercode,\n      }\n      return data;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n  },\n\n  methods: {\n    init(){\n      this.columns = dataConfig.columns;\n    },\n\n    //查询员工钱包交易记录信息\n    query(page) {\n      this.$store.commit('TABLE_LOADING', true);\n      let startDate = moment(this.date[0]).format('YYYY-MM-DD');\n      let endDate = moment(this.date[1]).format('YYYY-MM-DD');\n      if (startDate == '' || startDate == 'Invalid date' || endDate == '' || endDate == 'Invalid date'){\n        this.$Message.error(\"请选择交易日期起止！\");\n        return;\n      }\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        page: page || 1,\n        page_size: this.page_size,\n        ...this.resultForm\n      };\n\n      console.log(\"query--->>>\" + JSON.stringify(params))\n\n      // this.$store.dispatch('tradeUserList', params).then(res=>{\n      //   if (res.return_code == 0){\n      //     this.tableData = res.data.result;\n      //     this.total = res.data.total;\n      //   }\n      // }).catch(err => {});\n    },\n\n    handlePageDown(curPage){\n      this.query(curPage);\n    }\n  },\n\n  mounted(){\n    this.init();\n    this.query();\n  }\n}\n\n</script>\n<style lang='less' scoped>\n\n/deep/.ivu-table td.order_amount{\n  color: #ff602e;\n}\n/deep/.ivu-table-wrapper{\n  overflow: visible;\n}\n.refund{\n  /deep/.ivu-icon{\n    display: none;\n    left: 16px;\n  }\n}\n\n/deep/.ivu-poptip-title-inner{\n  color: #e42329;\n  padding: 10px;\n}\n</style>\n"]}]}