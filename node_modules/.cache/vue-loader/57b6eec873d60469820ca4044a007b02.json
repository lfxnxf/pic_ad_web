an>\n      <span class=\"blue-color\">已预约就餐数量</span>合计<span class=\"font-blod font-size16\"> {{statisticsList.o_a_count || 0}} </span>\n      <span>(占比{{statisticsList.o_a_rate}}%)。</span>\n    </div>\n    <Table border stripe :columns='columns' :data='statisticsList.result' :loading=\"loading\">\n     <template slot=\"o_n_rate\" slot-scope=\"{ row }\">\n       <span>{{row.o_n_rate}}%</span>\n     </template>\n     <template slot=\"o_a_rate\" slot-scope=\"{ row }\">\n       <span>{{ row.o_a_rate }}%</span>\n     </template>\n    </Table>\n  </div>\n</template>\n<script>\nimport _ from 'underscore';\nimport excel from '@/libs/excel';\nimport * as dataConfig from '../dataConfig';\nimport {buildChildDepartmentArray} from \"@/libs/util\";\nexport default {\n  name: 'SearchTable',\n  props: {\n    startDate: String,\n    endDate: String,\n  },\n  data() {\n    return {\n      columns: [],\n    };\n  },\n\n  components: {},\n\n  computed: {\n    statisticsList() {\n      return this.$store.state.statistics.statOrder;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n\n    // 导出\n    exportExcel: function() {\n      let pageSize = 100000;\n      let res = this.$parent.resultExport(pageSize);\n      // let res = this.$emit('resultExport',pageSize);\n      // console.log(\"exportExcel=====>>>>>\" + JSON.stringify(res));\n\n      try {\n        if (this.columns.length>0) {\n          let filename = '就餐统计表' + '_' + this.startDate + '-' + this.endDate;\n          let key = [];\n          for(let item of this.columns){\n            let tmp = ''\n            tmp = item.key? item.key: item.slot;\n            key.push(tmp);\n          }\n          const params = {\n            title: _.pluck(this.columns, 'title'),\n            key: key,\n            data: this.statisticsList.result,\n            autoWidth: true,\n            filename: filename,\n          };\n          excel.export_array_to_excel(params);\n        } else {\n          this.$Message.info('表格数据不能为空！');\n        }\n      } catch (e) {\n        console.log(e);\n        // this.$store.commit('STAT_DAY_SET_PROPERTY_BY_KEY', { key: 'exportLoading', value: false })\n      }\n    },\n\n  }\n}\n</script>\n<style lang='less' scoped>\n    /deep/.ivu-table td.order_amount{\n        color: #ff602e;\n    }\n</style>\n"]}]}