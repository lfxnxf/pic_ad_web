;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  async mounted() {\n    this.init();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n\n    handleEdit(row){\n      let data = {\n        device_id: row.device_id,\n        device_code: row.device_code,\n        name: row.name,\n        is_enable: row.is_enable.toString(),\n      }\n      this.$store.commit('setEditDeviceModal', true);\n      this.$store.commit('SetDeviceCurrentData', data);\n      console.log(\"===>deviceEditModal：\" + this.$store.state.device.editDeviceModal + \"， data：\" + JSON.stringify(data));\n\n    },\n\n  },\n\n}\n</script>\n<style lang='less' scoped>\n\n</style>\n"]}]}