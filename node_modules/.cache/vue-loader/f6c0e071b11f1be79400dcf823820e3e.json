utton>\n      </FormItem>\n    </Form>\n  </div>\n</template>\n<script>\nimport _ from 'underscore';\nexport default {\n  name: 'searchForm',\n  data() {\n    return {\n      loading: false,\n      form: {\n        key: '',\n        time_type: []\n      },\n    };\n  },\n\n  components: {},\n\n  computed: {\n    currentOrg() {\n      return JSON.parse(this.$store.state.user.currentOrg || '{}');\n    }\n\n  },\n\n  methods: {\n    query: async function(page, page_size) {\n      this.$store.commit('TABLE_LOADING', true);\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        key: this.form.key,\n        page: page || 1,\n        page_size: page_size || this.$parent.page_size\n      };\n\n      console.log('params ===> ', params)\n      await this.$store.dispatch('roleList', params);\n      this.$refs['formValidate'].resetFields();\n    },\n    // 显示添加人员类型页面\n    addRole() {\n      this.$store.commit('setAddRoleModal', true)\n    },\n\n  },\n  async mounted() {\n    await this.query();\n  },\n\n}\n</script>\n<style lang='less' scoped>\n.ivu-form-item{\n  margin-bottom: 0 !important;\n}\n</style>\n"]}]}