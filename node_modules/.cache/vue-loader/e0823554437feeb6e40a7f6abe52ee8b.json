n        <Button type='primary' icon=\"md-search\" @click=\"query()\">查询</Button>\n      </FormItem>\n      <FormItem>\n        <Button type='info' :loading=\"loading\" icon=\"md-add\" @click=\"addFood\">添加菜品</Button>\n      </FormItem>\n    </Form>\n  </div>\n</template>\n<script>\nimport _ from 'underscore';\nexport default {\n  name: 'searchForm',\n  data() {\n    return {\n      value: '123',\n      evTimeStamp : 0,\n      loading: false,\n      form: {\n        key: '',\n        time_type: []\n      },\n    };\n  },\n\n  components: {},\n\n  computed: {\n    currentOrg() {\n      return JSON.parse(this.$store.state.user.currentOrg || '{}');\n    },\n  },\n\n  methods: {\n\n    query: async function(page) {\n      // this.$store.commit('setEmployeeListLoading', true);\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        key: this.form.key,\n        time_type: this.form.time_type.toString(),\n        page: page || 1,\n        page_size: this.$parent.page_size\n      };\n      await this.$store.dispatch('goodsList', params);\n      // this.$store.commit('setEmployeeListLoading', false);\n      this.$refs['formValidate'].resetFields();\n    },\n    // 添加菜品\n    addFood() {\n      this.$store.commit('setAddModal', true)\n    }\n  },\n  mounted() {\n    this.query();\n  }\n}\n</script>\n<style lang='less' scoped>\n.ivu-form-item{\n  margin-bottom: 0 !important;\n}\n</style>\n"]}]}