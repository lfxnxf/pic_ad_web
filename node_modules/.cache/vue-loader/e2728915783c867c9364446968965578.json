: {\n    cardList() {\n      return this.$store.state.card.cardList;\n    },\n    loading() {\n      return this.$store.state.app.tableLoading;\n    },\n\n  },\n\n  async mounted() {\n    this.init();\n    await this.queryRole();\n  },\n\n  methods: {\n    init() {\n      this.columns = dataConfig.columns;\n    },\n\n    handleEdit(row){\n      let data = {\n        card_id: row.card_id,\n        card_no: row.card_no,\n        role_id: row.role_id,\n        role_name: row.role_name,\n        card_type: row.card_type.toString(),\n        card_type_desc: row.card_type_desc,\n        is_enable: row.is_enable.toString(),\n      }\n      this.$store.commit('setEditCardModal', true);\n      this.$store.commit('SETCARDCURRENTDATA', data);\n\n    },\n\n    //获取人员类型列表\n    queryRole: async function(page, page_size) {\n      let params = {\n        merchant_id: this.$store.state.user.curMerchantId,\n        is_enable: 1,\n        page: page || 1,\n        page_size: page_size || this.$parent.page_size\n      };\n      return await this.$store.dispatch('roleList', params);\n    },\n\n  },\n\n}\n</script>\n<style lang='less' scoped>\n\n</style>\n"]}]}