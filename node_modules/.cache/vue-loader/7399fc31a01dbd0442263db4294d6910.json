wTitle(parentItem) }}</span\n      ><Icon style=\"float: right;\" v-if=\"!hideTitle\" type=\"ios-arrow-forward\" :size=\"16\"\n    /></a>\n    <DropdownMenu ref=\"dropdown\" slot=\"list\">\n      <template v-for=\"child in children\">\n        <collapsed-menu\n          v-if=\"showChildren(child)\"\n          :icon-size=\"iconSize\"\n          :parent-item=\"child\"\n          :key=\"`drop-${child.name}`\"\n        ></collapsed-menu>\n        <DropdownItem v-else :key=\"`drop-${child.name}`\" :name=\"child.name\"\n          ><common-icon :size=\"iconSize\" :type=\"child.icon\" /><span class=\"menu-title\">{{\n            showTitle(child)\n          }}</span></DropdownItem\n        >\n      </template>\n    </DropdownMenu>\n  </Dropdown>\n</template>\n<script>\nimport mixin from './mixin';\nimport itemMixin from './item-mixin';\n// import { findNodeUpperByClasses } from '@/libs/util';\n\nexport default {\n  name: 'CollapsedMenu',\n  mixins: [mixin, itemMixin],\n  props: {\n    hideTitle: {\n      type: Boolean,\n      default: false,\n    },\n    rootIconSize: {\n      type: Number,\n      default: 16,\n    },\n  },\n  data() {\n    return {\n      placement: 'right-end',\n    };\n  },\n  methods: {\n    handleClick(name) {\n      this.$emit('on-click', name);\n    },\n    handleMousemove(event, children) {\n      const { pageY } = event;\n      const height = children.length * 38;\n      const isOverflow = pageY + height < window.innerHeight;\n      this.placement = isOverflow ? 'right-start' : 'right-end';\n    },\n  },\n  // mounted() {\n  //   let dropdown = findNodeUpperByClasses(this.$refs.dropdown.$el, [\n  //     'ivu-select-dropdown',\n  //     'ivu-dropdown-transfer',\n  //   ]);\n  //   if (dropdown) dropdown.style.overflow = 'visible';\n  // },\n};\n</script>\n<style lang=\"less\" scoped>\n  @import './side-menu.less';\n</style>\n\n"]}]}